        <call-macro n="set_ana"><with-param pos="4"/></call-macro>
        <choose>
          <when c="Only when upper case adverbs (first word of sent)">
            <test><or>
              <equal><clip pos="1" part="lemh"/><lit v="Adv"/></equal>
              <equal><clip pos="1" part="lemh"/><lit v="N"/></equal>
