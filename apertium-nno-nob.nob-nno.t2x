              <b/>
              <chunk><clip pos="4" part="whole"/></chunk>
              <chunk><clip pos="3" part="whole"/></chunk>
            </out>
          </when>
          <otherwise c="No change of order">
            <out>
              <chunk><clip pos="1" part="whole"/></chunk>
              <b/>
              <chunk><clip pos="2" part="whole"/></chunk>
              <b/>
              <chunk><clip pos="3" part="whole"/></chunk>
              <b/>
              <chunk><clip pos="4" part="whole"/></chunk>
            </out>
          </otherwise>
      </choose>
      </action>
    </rule>

