    <rule comment="DETPOS NIND: min mor => mora mi 
      nind makes sure we don't match 'min Per(np)' nor 'min bils(gen)' ">
      <pattern>
	<pattern-item n="detpos"/>    
	<pattern-item n="nind"/>
      </pattern>
      <action>
        <modify-case><clip pos="1" side="tl" part="lemh"/><clip pos="2" side="tl" part="lemh"/></modify-case>

        <out>
          <lu>
	    <get-case-from pos="1"> <clip pos="2" side="tl" part="lemh"/></get-case-from>
            <clip pos="2" side="tl" part="a_nom"/>
            <clip pos="2" side="tl" part="gen"/>
            <clip pos="2" side="tl" part="nbr"/>
	    <lit-tag v="def"/>
	    <clip pos="2" side="tl" part="lemq"/>
          </lu>
          <b pos="1"/>
          <lu>
	    <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="a_det"/>
	    <clip pos="2" side="tl" part="gen"/>  
            <clip pos="2" side="tl" part="nbr"/>
          </lu>
        </out>
      </action>
    </rule>



    <rule comment="DETPOS ADJ NOM: mitt lille navn => det vesle namnet mitt ">
      <pattern>
        <pattern-item n="detpos"/>
        <pattern-item n="adj"/>
        <pattern-item n="nom"/>
      </pattern>
      <action>
        <modify-case><clip pos="1" side="tl" part="lemh"/><clip pos="2" side="tl" part="lemh"/></modify-case>
	<call-macro n="set_number">
	  <with-param pos="1"/>
	</call-macro>
	<call-macro n="set_gender">
	  <with-param pos="3"/>
	</call-macro>
        <out>
          <lu>
	    <get-case-from pos="1"><lit v="de"/></get-case-from>
	    <lit-tag v="det.dem"/>
	    <var n="gender"/>
            <clip pos="1" side="tl" part="nbr"/>
          </lu>
	  <b pos="1"/>
          <lu>
            <clip pos="2" side="tl" part="whole"/>
          </lu>
	  <b pos="2"/>
          <lu>
            <clip pos="3" side="tl" part="lemh"/>
            <clip pos="3" side="tl" part="a_nom"/>
            <clip pos="3" side="tl" part="gen"/>
            <clip pos="1" side="tl" part="nbr"/>
      	    <lit-tag v="def"/>
	          <clip pos="3" side="tl" part="lemq"/>
          </lu>
	  <b/>
          <lu>
      	    <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="a_det"/>
	    <!-- TODO: gen=mf should depend on noun gender on the
	         nynorsk side since bokmål min is mf while nynorsk is
	         m -->
            <clip pos="3" side="tl" part="gen"/>  
            <clip pos="1" side="tl" part="nbr"/>
          </lu>
        </out>
      </action>
    </rule>


    <rule comment="DET: en => ein
		   Frequency is on our side, but politically incorrect..hmm..">
      <pattern>
        <pattern-item n="det"/>
      </pattern>
      <action>
        <call-macro n="set_number">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="set_gender">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="a_det"/>
            <var n="gender"/>
            <var n="number"/>
            <clip pos="1" side="tl" part="art"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule comment="DET NOM: en kvinne => ei kvinne
		            hvilken jente => kva for ei jente
			    egne hus => eigne hus">
      <pattern>
        <pattern-item n="det"/>
        <pattern-item n="nom"/>
      </pattern>
      <action>
        <call-macro n="set_number">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="set_gender">
          <with-param pos="2"/>
        </call-macro>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="a_det"/>
            <var n="gender"/>
	    <!-- determiners are less ambiguous wrt. nbr -->
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="art"/>
<!-- 	    <clip pos="1" side="tl" part="cas"/> -->
          </lu>
          <b pos="1"/>
          <lu>
	    <clip pos="2" side="tl" part="lemh"/>
            <clip pos="2" side="tl" part="a_nom"/>
            <clip pos="2" side="tl" part="gen"/>
            <clip pos="2" side="tl" part="nbr"/>
            <clip pos="2" side="tl" part="art"/>
<!-- 	    <clip pos="2" side="tl" part="cas"/> -->	
	    <clip pos="2" side="tl" part="lemq"/>
          </lu>
        </out>
      </action>
    </rule>


    <rule comment="ADJ: åpne (hus) => opne (hus)
		   We let CG disambiguate art and nbr using the following noun,
		   o/w all info should be in the adjective wordform (eg. in the above
		   example, hus being ind should give plural).">
      <pattern>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="set_number">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="set_gender">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="a_adj"/>
            <var n="gender"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="art"/>
<!-- 	    <clip pos="1" side="tl" part="cas"/> -->	
          </lu>
        </out>
      </action>
    </rule>

  </section-rules>
</transfer>
