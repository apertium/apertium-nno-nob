<transfer>

  <section-def-cats>
    <def-cat n="nom">
      <cat-item tags="n.*"/>
      <cat-item tags="np.*"/>
    </def-cat>
    <def-cat n="nind">
      <cat-item tags="n.*.*.ind"/>
    </def-cat>
    <def-cat n="navn">					       
      <cat-item tags="np.*"/>
    </def-cat>
    <def-cat n="det">
      <cat-item tags="det.*"/>
    </def-cat>
    <def-cat n="detpos">			      <!-- min sin -->
      <cat-item tags="det.pos.*"/>
    </def-cat>
    <def-cat n="adj">
      <cat-item tags="adj.*"/>
    </def-cat>
    <def-cat n="vblexpass">
      <cat-item tags="vblex.pass.*"/>
    </def-cat>
  </section-def-cats>

  
  <section-def-attrs>
    <def-attr n="a_nom">
      <attr-item tags="n"/>
    </def-attr>

    <def-attr n="a_adj">
      <attr-item tags="adj"/>
      <attr-item tags="adj.pst"/>
      <attr-item tags="adj.comp"/>
      <attr-item tags="adj.sup"/>
    </def-attr>

    <def-attr n="persona">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
    </def-attr>

    <def-attr n="a_verb">
      <attr-item tags="vblex"/>
      <attr-item tags="vber"/>
      <attr-item tags="vbhar"/>
      <attr-item tags="vbmod"/>
      <attr-item tags="vbaux"/>
      <attr-item tags="vbs"/>
    </def-attr>

    <def-attr n="a_inf">
      <attr-item tags="vblex.prs"/>
      <attr-item tags="vber.prs"/>
      <attr-item tags="vbhar.prs"/>
      <attr-item tags="vbmod.prs"/>
      <attr-item tags="vbaux.prs"/>
      <attr-item tags="vbs.prs"/>
    </def-attr>
    <def-attr n="temps">
      <attr-item tags="inf"/>
      <attr-item tags="prs"/>
      <attr-item tags="da"/>
      <attr-item tags="part"/>
    </def-attr>
    <def-attr n="a_det">
      <attr-item tags="det"/>				       <!-- selve<det><def> -->
      <attr-item tags="det.forst"/>
      <attr-item tags="det.dem"/>
      <attr-item tags="det.itg"/>
      <attr-item tags="det.qnt"/>
      <attr-item tags="det.pos"/>
    </def-attr>
    <def-attr n="a_prn">
      <attr-item tags="prn.enc"/>
      <attr-item tags="prn.enc.ref"/>
    </def-attr>
    <def-attr n="gen">
      <attr-item tags="m"/>
      <attr-item tags="mf"/>
      <attr-item tags="nt"/>
      <attr-item tags="f"/>
      <attr-item tags="GD"/>
    </def-attr>
    <def-attr n="nbr">
      <attr-item tags="sg"/>
      <attr-item tags="pl"/>
      <attr-item tags="sp"/>
    </def-attr>
    <def-attr n="cas">
      <attr-item tags="nom"/>
      <attr-item tags="gen"/>
      <attr-item tags="acc"/>
    </def-attr>
    <def-attr n="art">
      <attr-item tags="def"/>
      <attr-item tags="ind"/>
    </def-attr>
  </section-def-attrs>


  <section-def-vars>
    <def-var n="gender"/>     <!-- by set_m_iff_mf_sg or set_mf_or_nt -->
    <def-var n="number"/>
  </section-def-vars>


  <section-def-macros>

    <def-macro n="f_bcond" npar="1">
      <!--Per mirar si el blanc conté o no format awhatnow? -->
      <choose>
        <when>
          <test>
            <not>
              <equal><b pos="1"/><lit v=" "/></equal>
            </not>
          </test>
          <out><b pos="1"/></out>
        </when>
      </choose>
    </def-macro>

    <def-macro n="set_number" npar="1">
      <let><var n="number"/><clip pos="1" side="sl" part="nbr"/></let>
    </def-macro>

    <def-macro n="set_gender" npar="1">
      <!-- arg 1: det, arg 1: possibly nom 
	   - iff sg and mf/GD, take a wild guess and go for m;
	   - plurals get no gender;
	   - if incongruent number, take number of det;
	   - o/w take what we're given (possibly from what we concord with,
	     possibly nothing, eg. with comparative adjectives) -->
      <choose>
        <when>
	  <test>
	    <and>
	      <equal><var n="number"/><lit-tag v="sg"/></equal>
              <or>
  	        <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="GD"/></equal>
  	        <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="mf"/></equal>
              </or>
	    </and>
	  </test>
	  <let><var n="gender"/><lit-tag v="m"/></let>
        </when>
	<when>
	  <test>
	    <equal><var n="number"/><lit-tag v="pl"/></equal>
	  </test>
	  <let><var n="gender"/><lit v=""/></let>
	</when>
	<otherwise>
	  <let><var n="gender"/><clip pos="1" side="sl" part="gen"/></let>
	</otherwise>
      </choose>
    </def-macro>

    <def-macro n="set_mf_or_nt" npar="1">
      <!-- - iff m or f, use mf
           - o/w use nt. For eg. 'den/det' -->
      <choose>
        <when>
	  <test>
	      <or>
		<equal><clip pos="1" side="sl" part="gen"/><lit-tag v="m"/></equal>
		<equal><clip pos="1" side="sl" part="gen"/><lit-tag v="f"/></equal>
	      </or>
	  </test>
	  <let><var n="gender"/><lit-tag v="mf"/></let>
        </when>
	<otherwise>
	  <let><var n="gender"/><lit-tag v="nt"/></let>
	</otherwise>
      </choose>
    </def-macro>

  </section-def-macros>

