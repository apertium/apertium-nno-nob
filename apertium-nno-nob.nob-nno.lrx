      <match lemma="tour" tags="n.*"/>
      <match lemma="tur" tags="n.*"/>
    </or>
  </rule>
  <rule weight="3.0" c="opp 3 prosent i løpet av dagen → gjennom">
    <match tags="*"/>
    <match lemma="i løpet av" tags="pr"><select lemma="gjennom"/></match>
    <or>
      <match lemma="time"/>
