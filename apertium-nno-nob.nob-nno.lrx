	  <match lemma="OL" tags="n.*"/>
	  <match lemma="ritt" tags="n.*"/>
	  <match lemma="etappe" tags="n.*"/>
	</or>
  </rule>
  <rule weight="2.4" c="framme og bak">
    <match lemma="foran" tags="pr.*"><select lemma="framme"/></match>
  <match lemma="og"/>
  <match lemma="bak"/>
  </rule>
   <rule weight="2.4" c="bak og framme">
    <match lemma="bak"/>
    <match lemma="og"/>
    <match lemma="foran" tags="pr.*"><select lemma="framme"/></match>
