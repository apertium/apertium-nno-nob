<rules>
  <!-- NOB → NNO -->

  <rule><match lemma="forhold" tags="n.*"><select lemma="forhold"/></match></rule>
  <!-- <rule weight="2.0"> -->
  <!--   <match tags="n.*.*.*.cmp"/> -->
  <!--   <match lemma="forhold" tags="n.*"> -->
  <!--     <select lemma="forhold"/> -->
  <!--   </match> -->
  <!-- </rule> -->


  <rule> <match lemma="kjennelse" tags="n.*"> <select lemma="rettsavgjerd"/> </match> </rule>
  <rule weight="2.0">
    <or>
      <match lemma="rett"/>
      <match lemma="rettslig"/>
      <match lemma="Høyesterett"/>
      <match lemma="høyesterett"/>
    </or>
    <match lemma="kjennelse" tags="n.*">
      <select lemma="avgjerd"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="kjennelse" tags="n.*">
      <select lemma="avgjerd"/>
    </match>
    <or>
      <match lemma="rett"/>
      <match lemma="rettslig"/>
      <match lemma="Høyesterett"/>
      <match lemma="høyesterett"/>
    </or>
  </rule>
  <rule weight="2.0">
    <match lemma="kjennelse" tags="n.*">
      <select lemma="avgjerd"/>
    </match>
    <match tags="*"/>
    <or>
      <match lemma="rett"/>
      <match lemma="rettslig"/>
      <match lemma="Høyesterett"/>
      <match lemma="høyesterett"/>
    </or>
  </rule>
  <rule weight="2.0">
    <or>
      <match lemma="rett"/>
      <match lemma="rettslig"/>
      <match lemma="Høyesterett"/>
      <match lemma="høyesterett"/>
    </or>
    <match tags="*"/>
    <match lemma="kjennelse" tags="n.*">
      <select lemma="avgjerd"/>
    </match>
  </rule>

  <rule> <match lemma="rystelse" tags="n.*"> <select lemma="risting"/> </match> </rule>
  <rule weight="2.0">
    <or>
      <match lemma="oppleve"/>
      <match lemma="fornemme"/>
      <match lemma="kjenne"/>
      <match lemma="nok"/>
      <match lemma="være"/>
      <match lemma="bli"/>
      <match lemma="blive"/>
    </or>
    <match tags="*"/>
    <match lemma="rystelse" tags="n.*">
      <select lemma="skaking"/>
    </match>
  </rule>
  <rule weight="2.0">
    <or>
      <match lemma="være"/>
      <match lemma="bli"/>
      <match lemma="blive"/>
    </or>
    <match lemma="denne"/>
    <match lemma="en"/>
    <match lemma="rystelse" tags="n.*">
      <select lemma="skaking"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="rystelse" tags="n.*">
      <select lemma="skaking"/>
    </match>
    <match lemma="for"/>
  </rule>

  <rule> <match lemma="bety" tags="vblex.*"> <select lemma="bety"/> </match> </rule>

  <!-- I have yet to see non-erroneous uses of these words as masc in corpus -->
  <rule> <match lemma="formannskap">   <select lemma="formannskap"   tags="n.nt.*"/> </match> </rule>
  <rule> <match lemma="presidentskap"> <select lemma="presidentskap" tags="n.nt.*"/> </match> </rule>
  <rule> <match lemma="borgerskap">    <select lemma="borgarskap"    tags="n.nt.*"/> </match> </rule>

  <rule> <match lemma="være" tags="vblex.*"> <select lemma="vere"/> </match> </rule>
  <rule weight="2.0" c="er altså Johaug også blitt">
    <match lemma="være" tags="vblex.*">
      <select lemma="ha"/>
    </match>
    <repeat from="0" upto="3"><match/></repeat>
    <or>
      <match lemma="bli" tags="vblex.pp"/>
      <match lemma="bli" tags="adj.pp.*"/>
      <match lemma="blive" tags="vblex.pp"/>
      <match lemma="blive" tags="adj.pp.*"/>
      <match lemma="komme" tags="vblex.pp"/>
      <match lemma="komme" tags="adj.pp.*"/>
      <match lemma="foregå" tags="vblex.pp"/>
    </or>
  </rule>

  <rule> <match lemma="fremlegge" tags="vblex.*"> <select lemma="framlegge"/> </match> </rule>
  <rule> <match lemma="framlegge" tags="vblex.*"> <select lemma="framlegge"/> </match> </rule>

  <rule> <match lemma="anbefaling" tags="n.*"><select lemma="tilråding"/></match> </rule>
  <rule> <match lemma="anbefale" tags="vblex.*"> <select lemma="tilrå"/> </match> </rule>

  <rule> <match lemma="prøve" tags="vblex.*"> <select lemma="prøve"/> </match> </rule>
  <rule weight="2.0">
    <match lemma="prøve" tags="vblex.*">
      <select lemma="freiste"/>
    </match>
    <match lemma="lykke"/>
  </rule>
  <rule> <match lemma="friste" tags="vblex.*"> <select lemma="freiste"/> </match> </rule>
  <rule> <match lemma="forsøke" tags="vblex.*"> <select lemma="prøve"/> </match> </rule>

  <rule> <match lemma="domstolsbehandling" tags="n.*"> <select lemma="domstolsbehandling"/> </match> </rule>
  <rule> <match lemma="forening" tags="n.*">           <select lemma="foreining"/> </match> </rule>
  <rule> <match lemma="behandling" tags="n.*">         <select lemma="behandling"/> </match> </rule>
  <rule> <match lemma="forening" tags="n.*">           <select lemma="foreining"/> </match> </rule>
  <rule> <match lemma="verdighet" tags="n.*">          <select lemma="verd"/> </match> </rule>
  <rule> <match lemma="bedrift" tags="n.*">            <select lemma="bedrift"/> </match> </rule>
  <rule> <match lemma="befolkning" tags="n.*">         <select lemma="befolkning"/> </match> </rule>
  <rule weight="2.0">
    <match lemma="befolkning" tags="n.*">         <select lemma="befolkning"/> </match>
    <match tags="n.*.*.*.cmp"/>
  </rule>
  <rule weight="2.0">
    <match tags="n.*.*.*.cmp"/>
    <match lemma="befolkning" tags="n.*">         <select lemma="befolkning"/> </match>
  </rule>

  <rule> <match lemma="nødvendigvis" tags="adv"> <select lemma="nødvendigvis"/> </match> </rule>
  <rule> <match lemma="for øvrig" tags="adv"> <select lemma="elles"/> </match> </rule>
  <rule> <match lemma="muligens" tags="adv"> <select lemma="kanskje"/> </match> </rule>
  <rule> <match lemma="muligvis" tags="adv"> <select lemma="kanskje"/> </match> </rule>
  <rule> <match lemma="imidlertid" tags="adv"> <select lemma="likevel"/> </match> </rule>

  <rule> <match lemma="i påvente av at" tags="adv"> <select lemma="fram til"/> </match> </rule>

  <rule><match lemma="håpe" tags="vblex.*"><select lemma="håpe"/></match></rule>
  <rule><match lemma="håpe" tags="adj.*"><select lemma="håpe"/></match></rule>

  <rule> <match lemma="håp" tags="n.*"> <select lemma="håp"/> </match> </rule>
  <rule> <match lemma="forhåpning" tags="n.*"> <select lemma="von"/> </match> </rule>
  <rule weight="2.0">
    <match lemma="forhåpning" tags="n.*.*.def">
      <select lemma="håp"/>
    </match>
  </rule>

  <rule> <match lemma="bruk" tags="n.*"> <select lemma="bruk"/> </match> </rule>

  <rule> <match lemma="beherske" tags="vblex.*"> <select lemma="meistre"/> </match> </rule>

  <rule> <match lemma="bevare" tags="vblex.*"> <select lemma="verne"/> </match> </rule>
  <rule weight="2.0">
    <match lemma="bevare" tags="vblex.imp">
      <select lemma="bevare"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="bevare" tags="vblex.pp">
      <select lemma="bevare"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="bevare" tags="vblex.*.pasv">
      <select lemma="bevare"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="bevare" tags="adj.pp.*">
      <select lemma="bevare"/>
    </match>
  </rule>

  <rule> <match lemma="anklage" tags="n.*"> <select lemma="skulding"/> </match> </rule>
  <rule weight="2.0">
    <match lemma="anklage" tags="n.*.sg.*">
      <select lemma="tiltale"/>
    </match>
    <or>
      <match lemma="lyde"/>
      <match lemma="for"/>
    </or>
  </rule>

  <rule> <match lemma="anklage" tags="vblex.*"> <select lemma="skylde"/> </match> </rule>
  <rule> <match lemma="anklage" tags="vblex.*"> <select lemma="skulde"/> </match> </rule>
  <rule> <match lemma="anklage" tags="adj.*"> <select lemma="skylde"/> </match> </rule>
  <rule> <match lemma="anklage" tags="adj.*"> <select lemma="skulde"/> </match> </rule>

  <rule> <match lemma="skylde" tags="vblex.*"> <select lemma="skylde"/> </match> </rule>
  <rule weight="2.0"  c="skulde på"> 
    <match lemma="skylde" tags="vblex.*"> <select lemma="skulde"/> </match> 
    <match lemma="på"/>
  </rule>

  <rule> <match lemma="trygghet" tags="n.*"> <select lemma="tryggleik"/> </match> </rule>


  <!-- antall → mengd / tal / talet på -->
  <rule weight="1.0" c="default talet på">
    <match lemma="antall" tags="n.*"> <select lemma="talet på"/> </match>
  </rule>
  <rule weight="4.0">
    <match lemma="antall"><select lemma="talet på"/></match>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>
  <rule weight="3.0">
    <match lemma="antall"><select lemma="talet på"/></match>
    <or>
      <match lemma="krone"/>
      <match lemma="grad"/>
    </or>
  </rule>

  <rule weight="2.0" c="Uavhengig av antall gir de folkene i departementene mer enn nok å gjøre">
    <match lemma="antall">
      <select lemma="mengde"/>  <!-- or just "tal"? -->
    </match>
    <or>                        <!-- vblex not pp -->
      <match tags="vblex.pret"/>
      <match tags="vblex.pres"/>
      <match tags="vblex.imp"/>
      <match tags="vblex.pasv.pres"/>
      <match tags="vblex.pasv.inf"/>
      <match tags="cnjsub"/>
    </or>
  </rule>

  <rule weight="9.0" c="opptaksantallet → opptakstalet (compounds always «tal»)">
    <match tags="n.*.cmp"/>
    <match lemma="antall" tags="n.*"> <select lemma="tal"/> </match>
  </rule>
  <rule weight="2.0">
    <or>
      <match lemma="stor"/>
      <match lemma="beskjeden"/>
    </or>
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="2.0">
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
    <or>
      <match tags="sent"/>
      <match tags="sent.*"/>
      <match tags="rquot"/>
      <match tags="rpar"/>
      <match tags="cm"/>
      <match tags="cm.*"/>
    </or>
  </rule>
  <rule weight="2.0">
    <match lemma="antall" tags="n.*.sg.def">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="5.0" c="det antallet → det talet">
    <or>
      <match lemma="den" tags="det.dem.nt.sg"/>
      <match lemma="denne" tags="det.dem.nt.sg"/>
      <match lemma="denne" tags="prn.dem.nt.sg.*"/>
    </or>
    <match lemma="antall" tags="n.*.sg.def">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="8.0" c="et stort antall → eit stort tal">
    <match lemma="en" tags="det.qnt.nt.sg"/>
    <match tags="adj.*"/>
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="8.0" c="et langt større antall → eit langt større tal">
    <match lemma="en" tags="det.qnt.nt.sg"/>
    <or>
      <match tags="adv"/>
      <match tags="adj.*.nt.sg.ind"/>
    </or>
    <match tags="adj.*"/>
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="9.1" c="i et antall av → i eit tal av">
    <match lemma="i"/>
    <match lemma="en"/>
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
    <match lemma="av"/>
  </rule>
  <rule weight="5.2" c="et antall → eit tal">
    <match lemma="en" tags="det.qnt.nt.sg"/>
    <match lemma="antall">
      <select lemma="tal"/>
    </match>
  </rule>
  <rule weight="4.0" c="då har antallet vore søkkande
                        I løpet av de siste ti årene har antallet steget kraftig">
    <or>
      <match lemma="ha"/>
      <match lemma="ønske" tags="vblex.*"/> <!-- Domstoladministrasjonen ønsker antallet redusert med 30 -->
    </or>
    <match lemma="antall" tags="n.*.sg.def">
      <select lemma="tal"/>
    </match>
    <or>
      <match lemma="være"/>
      <match tags="adj.pp.*"/>
      <match tags="vblex.pp.*"/>
    </or>
  </rule>

  <rule weight="9.0" c="ha et antall → ha fleire; over et antall → over fleire">
    <or>
      <match lemma="ha" tags="vblex.*"/>
      <match tags="pr"/>
      <match lemma="se# på"/>   <!-- TODO all mwe-verbs ending in pr's … -->
    </or>
    <match lemma="en" tags="det.qnt.nt.sg"/>
    <match lemma="antall">
      <select lemma="fleire"/>
    </match>
  </rule>

  <rule weight="8.0" c="et antall av → ein del av">
    <match lemma="en" tags="det.qnt.nt.sg"/>
    <match lemma="antall">
      <select lemma="del"/>
    </match>
    <match lemma="av"/>
  </rule>

  <rule weight="8.0">
    <match lemma="den"/>
    <or>
      <match lemma="høy"/>
      <match lemma="stor"/>
    </or>
    <match lemma="antall"><select lemma="tal"/></match>
  </rule>
  <rule weight="8.0">
    <match lemma="den"/>
    <match lemma="total" tags="adj.*"/>
    <match lemma="antall"><select lemma="tal"/></match>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

  <rule><match lemma="av det totale antallet"><select lemma="av det samla talet"/></match></rule>
  <rule weight="2.0" c="av det totalet antallet beslag → av alle beslag">
    <match lemma="av det totale antallet">
      <select lemma="av alle"/>
    </match>
