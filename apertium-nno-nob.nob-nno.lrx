      <match lemma="oktober" tags="n.m.sg.ind"/>
      <match lemma="november" tags="n.m.sg.ind"/>
      <match lemma="desember" tags="n.m.sg.ind"/>
      <match lemma="dag" tags="n.m.sg.def"/> <!-- or gjennom? -->
      <match lemma="år" tags="n.m.sg.def"/> <!-- or gjennom? -->
