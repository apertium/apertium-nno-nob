      <match tags="vblex.*"/>
      <match tags="adv"/>
    </or>
    <match lemma="meget" tags="adv"><select lemma="mykje" tags="adv"/></match>
	<or>
      <match tags="sent.clb"/>
	</or>
  </rule>
  <!-- END: meget, todo: "han reiste meget hele året rundt" -->
  
  <!-- START: ukentlig -->
  <rule weight="2.0" c="bussen går kvar veke">
    <match lemma="ukentlig" tags="adv"><select lemma="kvar veke"/></match>
  </rule>
  <rule weight="2.0" c="bussen går kvar veke">
    <match lemma="ukentlig" tags="adj.*.*.sg.ind"><select lemma="kvar veke"/></match>
	<or>
	  <match tags="pr"/>
	  <match tags="vblex.*"/>
      <match tags="cnjcoo"/>
      <match tags="sent.clb"/>
	</or>
  </rule>
  <rule weight="2.0" c="bussen går kvar veke">
	<or>
	  <match tags="pr"/>
	  <match tags="vblex.*"/>
      <match tags="cnjcoo"/>
      <match tags="adv"/>
	</or>
    <match lemma="ukentlig" tags="adj.*.*.sg.ind"><select lemma="kvar veke"/></match>
  </rule>
  <rule weight="4.0" c="på vekefast basis">
	<match tags="pr"/>
    <match lemma="ukentlig" tags="adj.*"><select lemma="vekefast"/></match>
  </rule>
  <rule weight="4.0" c="det vekefaste supermøtet">
  	<match tags="det.dem.*"/>
    <match lemma="ukentlig" tags="adj.*"><select lemma="vekefast"/></match>
  </rule>
  <rule weight="2.0" c="det vekefaste møtet">
    <match lemma="ukentlig" tags="adj.*"><select lemma="vekefast"/></match>
	<match tags="n.*.*.def"/>
  </rule>
  <rule  weight="2.0" c="eit vekevis opplegg">
    <match lemma="ukentlig" tags="adj.*"><select lemma="vekevis"/></match>
	<match tags="n.*.*.ind"/>
  </rule>
  <!-- END: ukentlig, todo: haltande adjektivløysingar, særleg "vekevis", ofte overflødig? -->
  
  <rule><match lemma="hva" tags="prn.*"><select lemma="kva"/></match></rule>
  <rule weight="2.0">
    <match lemma="i"/>
    <match lemma="strid"/>
    <match lemma="med"/>
    <match lemma="hva" tags="prn.itg"><select lemma="det"/></match>
  </rule>

  <rule><match lemma="noe"><select lemma="noko"/></match></rule>
  <rule weight="2.0" c="ikkje først i heilsetninga">
    <repeat from="2" upto="2"><seq n="NOT_sent_lpar_lquot_rquot_cnj"/></repeat>
    <match lemma="for"/>
    <match lemma="noe"><select lemma="noko"/></match>
  </rule>
  <rule weight="2.0">
    <match lemma="for"/>
    <match lemma="noe" tags="det.qnt.un.pl"><select lemma="somme"/></match>
    <match lemma="av"/>
  </rule>
  <rule weight="2.0">
    <match lemma="for"/>
    <match lemma="noe" tags="prn.ind.un.pl.*"><select lemma="somme"/></match>
    <or>
      <match lemma="være"/>
      <match lemma="bli"/>
      <match lemma="ha"/>
      <match lemma="føre"/>
    </or>
  </rule>

  <rule><match lemma="rettighetsforkjemper" tags="n.*"><select lemma="rettferdskjempar"/></match></rule>
  <rule weight="2.0">
    <match tags="n.*.cmp"/>
    <match lemma="rettighetsforkjemper" tags="n.*"><select lemma="rettsforkjempar"/></match>
  </rule>

  <rule><match lemma="splittelse" tags="n.*"><select lemma="usemje"/></match></rule>
  <rule weight="2.0">
    <match tags="n.*.cmp"><select lemma="splitting"/></match>
    <match lemma="splittelse" tags="n.*"><select lemma="splitting"/></match> <!-- eller er usemje betre i cmp òg? -->
  </rule>
</lrx>
