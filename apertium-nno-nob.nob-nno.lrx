      <match lemma="2018"/>
      <match lemma="2019"/>
      <match lemma="2020"/>
      <match lemma="2021"/>
      <match lemma="2022"/>
    </or>
  </rule>
  <rule weight="2.0" c="i løpet av de første årene → ∅">
    <match lemma="i løpet av" tags="pr"><select lemma=""/></match>
    <match lemma="den" tags="det.dem.un.pl"/>
  </rule>
