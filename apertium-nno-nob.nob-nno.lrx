  <rule><match lemma="myndighetsnivå" tags="n.*"><select lemma="styringsnivå"/></match></rule>
  <rule weight="2.0" c="møte nokon på styresmaktsnivå">
    <match lemma="på"/>
    <match lemma="myndighetsnivå" tags="n.nt.*.ind"><select lemma="styresmaktsnivå"/></match>
  </rule>

  <rule><match lemma="synes" tags="vblex.*"><select lemma="synast"/></match></rule>

  <rule><match lemma="uansett om" tags="adv"><select lemma="sjølv om"/></match></rule>
  <rule weight="2.0" c="anten ein liker det eller ikkje">
    <match lemma="uansett om" tags="adv"><select lemma="anten"/></match>
	<repeat from="0" upto="12"><seq n="NOT_sent_lpar_lquot_rquot"/></repeat>
	<match lemma="eller"/>
  </rule>

  <rule><match lemma="tilberedning" tags="n.*"><select lemma="tillaging"/></match></rule>
  
  <rule><match lemma="drøye" tags="adv"><select lemma="vel"/></match></rule>
  <rule><match lemma="drøyt" tags="adv"><select lemma="vel"/></match></rule>
  <rule weight="2.0" c="øya ligg godt og vel fem kilometer frå kysten">
    <or>
	  <match tags="vblex.*"/>
	  <match tags="adj.pp.*"/>
	  <match tags="adv"/>
	</or>
	<or>
      <match lemma="drøye" tags="adv"><select lemma="litt over"/></match>
      <match lemma="drøyt" tags="adv"><select lemma="litt over"/></match>
	</or>
  </rule>
  <rule weight="2.0" c="øya ligg godt og vel fem kilometer frå kysten">
    <or>
	  <match lemma="på" tags="pr"/>
	  <match tags="cm"/>
	</or>
	<or>
      <match lemma="drøye" tags="adv"><select lemma="godt og vel"/></match>
      <match lemma="drøyt" tags="adv"><select lemma="godt og vel"/></match>
	</or>
  </rule>
  
  <rule><match lemma="anslagsvis" tags="adv"><select lemma="anslagsvis"/></match></rule>
  <rule weight="2.0" c="rundt rekna tre millionar">
    <match lemma="anslagsvis" tags="adv"><select lemma="om lag"/></match>
	<match tags="det.qnt.*"/>
  </rule>
  <rule weight="2.0" c="rundt rekna tre millionar">
	<match tags="cm"/>
    <match lemma="anslagsvis" tags="adv"><select lemma="rundt rekna"/></match>
  </rule>
  
  <rule><match lemma="tidligere" tags="adv"><select lemma="tidlegare"/></match></rule>
  <rule weight="2.0" c="enn før">
    <or>
      <match lemma="enn"/>
      <match lemma="fra"/>
      <match lemma="som"/>
	</or>
    <match lemma="tidligere" tags="adv"><select lemma="før"/></match>
	<or>
	  <match tags="cm"/>
	  <match tags="cm.clb"/>
	  <match tags="sent.clb"/>
	</or>
  </rule>

  <rule><match lemma="tulling" tags="n.*"><select lemma="tulling" tags="n.m.*"/></match></rule>

  <rule><match lemma="begå" tags="vblex.*"><select lemma="gjere"/></match></rule>
  <rule weight="2.0" c="vere/bli gjort">
    <or>
      <match lemma="være" tags="vblex.*"/>
      <match lemma="bli" tags="vblex.*"/>
	</or>
    <match lemma="begå" tags="vblex.pp"><select lemma="gjere"/></match>
  </rule>
  <rule weight="2.0" c="blir gjort">
    <match lemma="begå" tags="vblex.pres.pasv"><select lemma="gjere"/></match>
  </rule>
  <rule weight="2.0" c="gjere seg skuldig i">
    <match lemma="begå" tags="vblex.*"><select lemma="gjere# seg skuldig i"/></match>
	<or>
	  <match tags="n.*"/>
	  <match tags="adj.*"/>
	  <match tags="det.qnt.*.sg"/>
	</or>
  </rule>
  
  <rule><match lemma="oppsummere" tags="vblex.*"><select lemma="oppsummere"/></match></rule>
  <rule weight="2.0" c=", oppsummerte Giske.">
  	<match tags="cm.*"/>
    <match lemma="oppsummere" tags="vblex.*"><select lemma="oppsummere"/></match>
  </rule>
  <rule weight="2.0" c="blir oppsummert">
    <match lemma="oppsummere" tags="vblex.pres.pasv"><select lemma="oppsummere"/></match>
  </rule>
  <rule weight="2.0" c="NN summerte opp rapporten">
    <match lemma="oppsummere" tags="vblex.*"><select lemma="summere# opp"/></match>
	<or>
	  <match tags="n.*.*.def"/>
	  <match tags="n.*.pl.ind"/>
	  <match tags="np.org"/>
	</or>
  </rule>
  <rule weight="2.0" c="Finanstilsynet summerer opp dei nye reglane slik">
    <match lemma="oppsummere" tags="vblex.*"><select lemma="summere# opp"/></match>
	  <match tags="det.dem.*"/>
	  <match tags="adj.*"/>
	  <match tags="n.*"/>
  </rule>
  
  <rule><match lemma="kreve" tags="vblex.*"><select lemma="krevje"/></match></rule>
  <rule weight="2.0" c="det som krevst">
    <match lemma="kreve" tags="vblex.pres.pasv"><select lemma="krevjast"/></match>
  </rule>
  <rule weight="2.5" c="det som krevst av/for">
    <match lemma="kreve" tags="vblex.pres.pasv"><select lemma="krevjast"/></match>
	<or>
	  <match lemma="for"/>
	  <match lemma="av"/>
	  <match lemma="fra"/>
	  <match lemma="etter"/>
	</or>
  </rule>
  <rule weight="2.8" c="det som trengst for å">
    <match lemma="kreve" tags="vblex.pres.pasv"><select lemma="trengast"/></match>
	<match lemma="for"/>
	<or>
	  <match lemma="å"/>
	  <match lemma="at"/>
	</or>
  </rule>
  <rule weight="2.3" c="det trengst i">
    <match lemma="kreve" tags="vblex.pres.pasv"><select lemma="trengast"/></match>
	<or>
  	  <match tags="pr"/>
  	  <match tags="adj"/>
	</or>
  </rule>
  <rule weight="2.9" c="det trengst ein">
    <match lemma="kreve" tags="vblex.pres.pasv"><select lemma="trengast"/></match>
  	<match tags="det.qnt.*.sg"/>
  </rule>
  
  <rule><match lemma="lide" tags="vblex.*"><select lemma="lide"/></match></rule>
  <rule><match lemma="li" tags="vblex.*"><select lemma="lide"/></match></rule>
  <rule weight="2.5" c="det lei mot kveld">
    <match lemma="li" tags="vblex.*"><select lemma="li"/></match>
	<or>
	  <match lemma="mot"/>
	  <match lemma="utpå"/>
	</or>
  </rule>
  <rule weight="2.0" c="lakke og li">
	<match lemma="lakke" tags="vblex.*"/>
	<match lemma="og"/>
    <match lemma="li" tags="vblex.*"><select lemma="li"/></match>
  </rule>

  <rule><match lemma="utsending" tags="n.m.*"><select lemma="utsending" tags="n.m.*"/></match></rule>
  <rule weight="2.0" c="utsendinga av">
    <match lemma="utsending" tags="n.m.*"><select lemma="utsending" tags="n.f.*"/></match>
	<match lemma="av"/>
  </rule>
  
  <rule><match lemma="spesialutsending" tags="n.m.*"><select lemma="spesialutsending" tags="n.m.*"/></match></rule>
  <rule weight="2.0" c="spesialutsendinga av">
    <match lemma="spesialutsending" tags="n.m.*"><select lemma="spesialutsending" tags="n.f.*"/></match>
	<match lemma="av"/>
  </rule>
  
  <rule><match lemma="føle" tags="vblex.*"><select lemma="kjenne"/></match></rule>
  <rule><match lemma="føle" tags="adj.*"><select lemma="kjenne"/></match></rule>
  <rule weight="2.0" c="kjenne seg">
    <or>
      <match lemma="føle" tags="vblex.*"><select lemma="kjenne"/></match>
      <match lemma="føle" tags="adj.pp.*"><select lemma="kjenne"/></match>
	</or>
	<repeat from="0" upto="6"><match/></repeat>
    <seq n="ref-prn"/>
  </rule>
  <rule weight="3.0" c="føle seg fram">
    <or>
      <match lemma="føle" tags="vblex.*"><select lemma="føle"/></match>
      <match lemma="føle" tags="adj.pp.*"><select lemma="føle"/></match>
	</or>
	<repeat from="0" upto="6"><match/></repeat>
    <seq n="ref-prn"/>
	<or>
	  <match lemma="fram"/>
	  <match lemma="frem"/>
	</or>
  </rule>
  <rule weight="3.0" c="føler at">
    <or>
      <match lemma="føle" tags="vblex.*"><select lemma="føle"/></match>
      <match lemma="føle" tags="adj.pp.*"><select lemma="føle"/></match>
	</or>
	<match lemma="at"/>
  </rule>
  <rule weight="3.0" c="føle med (nokon)">
    <or>
      <match lemma="føle" tags="vblex.*"><select lemma="føle"/></match>
      <match lemma="føle" tags="adj.pp.*"><select lemma="føle"/></match>
	</or>
	<or>
	  <match lemma="med"/>
	  <match lemma="for"/>
	</or>
  </rule>
  <rule weight="3.0" c="følende">
    <match lemma="føle" tags="adj.pprs"><select lemma="føle"/></match>
  </rule>

  <rule><match lemma="følelse" tags="n.*"><select lemma="kjensle"/></match></rule>
  <rule weight="2.0" c="ha/kjenne på følelsen">
    <or>
      <match lemma="ha" tags="vblex.*"/>
      <match lemma="kjenne" tags="vblex.*"/>
	</or>
    <repeat from="0" upto="3"><match/></repeat>
	<match lemma="på"/>
    <match lemma="følelse" tags="n.*"><select lemma="følelse"/></match>
  </rule>
  
  <rule><match lemma="forestående" tags="adj.*"><select lemma="kommande"/></match></rule>
  <rule><match lemma="nær forestående" tags="adj.*"><select lemma="kommande"/></match></rule>
  <rule><match lemma="nært forestående" tags="adj.*"><select lemma="kommande"/></match></rule>
  <rule weight="2.0" c="eit snarleg bryllaup">
	<match tags="det.qnt.*"/>
    <repeat from="0" upto="1">
	  <match tags="adj.*"/>
	</repeat>
    <or>
      <match lemma="forestående" tags="adj.*"><select lemma="snarleg"/></match>
      <match lemma="nær forestående" tags="adj.*"><select lemma="snarleg"/></match>
      <match lemma="nært forestående" tags="adj.*"><select lemma="snarleg"/></match>
	</or>
  </rule>
  <rule weight="2.0" c="svært snarleg">
    <or>
	  <match lemma="svært"/>
	  <match lemma="meget"/>
    </or>
    <or>
      <match lemma="forestående" tags="adj.*"><select lemma="snarleg"/></match>
      <match lemma="nær forestående" tags="adj.*"><select lemma="snarleg"/></match>
      <match lemma="nært forestående" tags="adj.*"><select lemma="snarleg"/></match>
	</or>
  </rule>

  <rule><match lemma="i særdeleshet" tags="adv"><select lemma="særleg"/></match></rule>
  <rule weight="2.0" c="... Sverige ikkje minst.">
    <match lemma="i særdeleshet" tags="adv"><select lemma="ikkje minst"/></match>
	<or>
  	  <match tags="cm"/>
  	  <match tags="sent.*"/>
  	  <match lemma="–"/>
  	  <match tags="np.*"/>
	</or>
  </rule>
  
  <rule><match lemma="utstrakt" tags="adj.*"><select lemma="utstrekt"/></match></rule>
  <rule weight="2.0" c="utbreidd bruk av">
    <match lemma="utstrakt" tags="adj.*"><select lemma="utbreidd"/></match>
	<or>
	  <match lemma="bruk" tags="n.*"/>
	  <match lemma="kortbruk" tags="n.*"/>
	  <match lemma="tvangsbruk" tags="n.*"/>
	  <match lemma="misbruk" tags="n.*"/>
	</or>
  </rule>
  <rule weight="2.0" c="utbreidd bruk av">
    <match lemma="utstrakt" tags="adj.*"><select lemma="utbreidd"/></match>
    <match tags="n.*.*.*.cmp"/>
	<or>
	  <match lemma="bruk" tags="n.*"/>
	  <match lemma="misbruk" tags="n.*"/>
	</or>
  </rule>  
  <rule weight="2.0" c="omfattande samarbeid">
    <match lemma="utstrakt" tags="adj.*"><select lemma="omfattande"/></match>
	<or>
	  <match lemma="samarbeid" tags="n.*"/>
	  <match lemma="kontakt" tags="n.*"/>
	  <match lemma="vern" tags="n.*"/>
	  <match lemma="kontroll" tags="n.*"/>
	  <match lemma="konsekvens" tags="n.*"/>
	  <match lemma="påvirkning" tags="n.*"/>
	  <match lemma="virksomhet" tags="n.*"/>
	  <match lemma="testing" tags="n.*"/>
	  <match lemma="festing" tags="n.*"/>
	</or>
  </rule>
  <rule weight="2.0" c="omfattande næringsverksemd">
    <match lemma="utstrakt" tags="adj.*"><select lemma="omfattande"/></match>
    <match tags="n.*.*.*.cmp"/>
	<or>
	  <match lemma="samarbeid" tags="n.*"/>
	  <match lemma="kontakt" tags="n.*"/>
	  <match lemma="vern" tags="n.*"/>
	  <match lemma="kontroll" tags="n.*"/>
	  <match lemma="konsekvens" tags="n.*"/>
	  <match lemma="påvirkning" tags="n.*"/>
	  <match lemma="virksomhet" tags="n.*"/>
	  <match lemma="testing" tags="n.*"/>
	  <match lemma="festing" tags="n.*"/>
	</or>
  </rule>  
  <rule weight="2.0" c="omfattande internasjonal kriminalitet">
    <match lemma="utstrakt" tags="adj.*"><select lemma="omfattande"/></match>
	<match tags="adj.*"/>
  </rule>
  <rule weight="2.0" c="stor spreiing">
    <match lemma="utstrakt" tags="adj.*"><select lemma="stor"/></match>
	<or>
	  <match lemma="spredning" tags="n.*"/>
	  <match lemma="frykt" tags="n.*"/>
	  <match lemma="glede" tags="n.*"/>
	</or>
  </rule>

  <rule><match lemma="setting" tags="n.*"><select lemma="setting" tags="n.m.*"/></match></rule>
  <rule weight="2.0" c="setjing av">
    <match lemma="setting" tags="n.*"><select lemma="setting" tags="n.f.*"/></match>
	<match lemma="av"/>
  </rule>

  <rule><match lemma="bestige" tags="vblex.*"><select lemma="klatre# opp på"/></match></rule>
  <rule><match lemma="bestige" tags="vblex.pp"><select lemma="erobre"/></match></rule>
  <rule><match lemma="bestige" tags="adj.pp.*"><select lemma="erobre"/></match></rule>
  <rule weight="2.0" c="ha klatra opp på">
	<match lemma="ha" tags="vblex.*"/>
    <repeat from="0" upto="12"><seq n="NOT_sent_lpar_lquot_rquot"/></repeat>
    <match lemma="bestige" tags="vblex.pp"><select lemma="klatre# opp på"/></match>
  </rule>
  <rule weight="2.0" c="overta trona">
    <match lemma="bestige" tags="vblex.*"><select lemma="overta"/></match>
	<match lemma="trone"/>
  </rule>
  <rule weight="2.0" c="ha overteke trona">
	<match lemma="ha" tags="vblex.*"/>
    <repeat from="0" upto="12"><seq n="NOT_sent_lpar_lquot_rquot"/></repeat>
    <match lemma="bestige" tags="vblex.pp"><select lemma="overta"/></match>
  </rule>
  <rule weight="2.2" c="fjellet er erobra">
    <or>
      <match lemma="være"/>
      <match lemma="bli"/>
    </or>
    <repeat from="0" upto="12"><seq n="NOT_sent_lpar_lquot_rquot"/></repeat>
	<or>
      <match lemma="bestige" tags="vblex.pp"><select lemma="erobre"/></match>
      <match lemma="bestige" tags="adj.pp.*"><select lemma="erobre"/></match>
	</or>
  </rule>
  <rule weight="2.2" c="oksen går opp på kua">
    <match lemma="bestige" tags="vblex.*"><select lemma="gå# opp på"/></match>
    <or>
	  <match lemma="ku"/>
	  <match lemma="kvige"/>
	  <match lemma="hoppe"/>
	  <match lemma="merr"/>
	  <match lemma="søye"/>
	  <match lemma="tikke"/>
	  <match lemma="purke"/>
	  <match lemma="tispe"/>
	  <match lemma="binne"/>
	  <match lemma="hunn"/>
	  <match lemma="hundyr"/>
	  <match lemma="kjerring"/>
	  <match lemma="kjæreste"/>
	  <match lemma="frue"/>
	</or>
  </rule>
  <rule weight="2.2" c="ha gått opp på">
	<match lemma="ha" tags="vblex.*"/>
    <repeat from="0" upto="12"><seq n="NOT_sent_lpar_lquot_rquot"/></repeat>
    <match lemma="bestige" tags="vblex.pp"><select lemma="gå# opp på"/></match>
    <or>
	  <match lemma="ku"/>
	  <match lemma="kvige"/>
	  <match lemma="hoppe"/>
	  <match lemma="merr"/>
	  <match lemma="søye"/>
	  <match lemma="tikke"/>
	  <match lemma="purke"/>
	  <match lemma="tispe"/>
	  <match lemma="binne"/>
	  <match lemma="hunn"/>
	  <match lemma="hundyr"/>
	  <match lemma="kjerring"/>
	  <match lemma="kjæreste"/>
	  <match lemma="frue"/>
	</or>
  </rule>
  <rule weight="2.4" c="kua blir bedekt">
    <or>
	  <match lemma="ku"/>
	  <match lemma="kvige"/>
	  <match lemma="hoppe"/>
	  <match lemma="merr"/>
	  <match lemma="søye"/>
	  <match lemma="tikke"/>
	  <match lemma="purke"/>
	  <match lemma="tispe"/>
	  <match lemma="binne"/>
	  <match lemma="hunn"/>
	  <match lemma="hundyr"/>
	  <match lemma="kjerring"/>
	  <match lemma="kjæreste"/>
	  <match lemma="frue"/>
	</or>
    <or>
      <match lemma="være"/>
      <match lemma="bli"/>
    </or>
    <match lemma="bestige" tags="adj.pp.*"><select lemma="bedekke"/></match>
  </rule>
  <rule weight="2.4" c="då er kua bedekt">
    <or>
      <match lemma="være"/>
      <match lemma="bli"/>
    </or>
    <or>
	  <match lemma="ku"/>
	  <match lemma="kvige"/>
	  <match lemma="hoppe"/>
	  <match lemma="merr"/>
	  <match lemma="søye"/>
	  <match lemma="tikke"/>
	  <match lemma="purke"/>
	  <match lemma="tispe"/>
	  <match lemma="binne"/>
	  <match lemma="hunn"/>
	  <match lemma="hundyr"/>
	  <match lemma="kjerring"/>
	  <match lemma="kjæreste"/>
	  <match lemma="frue"/>
	</or>
    <match lemma="bestige" tags="adj.pp.*"><select lemma="bedekke"/></match>
  </rule>

</lrx>
