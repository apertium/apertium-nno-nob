	  <match lemma="cupkamp" tags="n.*"/>
	  <match lemma="mesterligakamp" tags="n.*"/>
	  <match lemma="kvalifiseringskamp" tags="n.*"/>
	  <match lemma="kvalifisering" tags="n.*"/>
	  <match lemma="oppgjør" tags="n.*"/>
	  <match lemma="serieoppgjør" tags="n.*"/>
	  <match lemma="cupoppgjør" tags="n.*"/>
