	  <match lemma="morgendag" tags="n.m.sg.def.gen.*"/>
	  <match lemma="år" tags="n.nt.sg.def.gen.*"/>
	  <match lemma="fjorår" tags="n.nt.sg.def.gen.*"/>
	  <match lemma="møte" tags="n.*"/>
	  <match lemma="arrangement" tags="n.*"/>
	  <match lemma="landsmøte" tags="n.*"/>
	  <match lemma="toppmøte" tags="n.*"/>
	  <match lemma="ministermøte" tags="n.*"/>
	  <match lemma="valg" tags="n.*"/>
	  <match lemma="forhandling" tags="n.*"/>
	  <match lemma="mekling" tags="n.*"/>
	  <match lemma="lønnsoppgjør" tags="n.*"/>
	  <match lemma="kamp" tags="n.*"/>
