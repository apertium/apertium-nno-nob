    <rule comment="PR">
      <pattern>
        <pattern-item n="pr"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <choose>
          <when c="Only output if bidix has a lemma in l">
            <test>
              <not><equal><clip pos="1" side="tl" part="lem"/><lit v=""/></equal></not>
            </test>
            <let>
              <var n="c_lemtag"/>
              <concat><lit v="&lt;"/><clip pos="1" side="tl" part="lemh"/><lit v="&gt;"/></concat>
            </let>
            <choose>
              <when>
                <test><or>
                  <and><in caseless="yes"><var n="prevlem"/><list n="noun_takes_i"/></in>  <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="i"/></equal></and>
                  <and><in caseless="yes"><var n="prevlem"/><list n="noun_takes_til"/></in><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="til"/></equal></and>
                </or></test>
                <let><var n="pr_head"/><lit-tag v="nhead"/></let>
              </when>
              <otherwise>
                <!-- By default no tag, assume verb head: -->
                <let><var n="pr_head"/><lit v=""/></let>
              </otherwise>
            </choose>
            <out>
              <chunk name="pr" case="caseFirstWord">
                <tags>
                  <tag><clip pos="1" side="tl" part="tags"/></tag>
                  <tag><var n="c_lemtag"/></tag>
                  <tag><var n="pr_head"/></tag>
                </tags>
                <lu><clip pos="1" side="tl" part="whole"/></lu>
              </chunk>
            </out>
            <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
            <let><var n="prevnbr"/><clip pos="1" side="tl" part="nbr"/></let>
          </when>
          <otherwise>
            <let><var n="prevDeletedCase"/><lit v="aa"/></let>
            <modify-case><var n="prevDeletedCase"/><var n="caseFirstWord"/></modify-case>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="I FORRIGE MÅNED: i neste uke => neste uke">
      <pattern>
        <pattern-item n="i"/>
        <pattern-item n="forrige_neste"/>
        <pattern-item n="nind_periode"/>
      </pattern>
      <action>
        <choose><when><test><not><equal><clip pos="3" side="tl" part="a_cmp"/><lit v=""/></equal></not></test><reject-current-rule shifting="no"/></when></choose>
        <choose><when c="Don't drop for 'utover i neste uke'">
          <test><or>
            <equal caseless="yes"><var n="prevlem"/><lit v="utover"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="ut"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="inn"/></equal>
          </or></test>
          <reject-current-rule shifting="no"/>
        </when></choose>
        <call-macro n="set_caseFirstWord3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <choose><when>
          <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
          <out><b pos="1"/></out>
        </when></choose>
        <out>
          <chunk name="det" case="caseFirstWord">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="2"/>
          <chunk name="nn">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

