            <equal><clip pos="4" side="sl" part="cas"/><lit-tag v="gen"/></equal>
            <not><equal><clip pos="4" side="sl" part="a_cmp"/><lit v=""/></equal></not>
            <equal><clip pos="6" side="sl" part="cas"/><lit-tag v="gen"/></equal>
