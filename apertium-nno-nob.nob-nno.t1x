        <test><and>
          <in caseless="yes"><var n="prevlem"/><list n="bi-indgen-owner"/></in>                    <!-- kvinnenes-->
          <in caseless="yes"><clip pos="1" side="tl" part="lemh"/><list n="bi-indgen-owned"/></in> <!-- kamp -->
        </and></test>
        <append n="ntags"><lit-tag v="gendefdrop"/></append>
