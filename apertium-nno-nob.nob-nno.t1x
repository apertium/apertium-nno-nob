    <def-macro n="out_gen-prep_from_var" npar="0"
               c="arg.1: proper noun
                  Assumes gen-prep variable has been set first">
      <let><var n="b"/><lit v=" "/></let>
      <out>
        <chunk name="gen-prep">
          <tags><tag><lit-tag v="pr"/></tag></tags>
          <lu>
            <var n="gen-prep"/>
            <lit-tag v="pr"/>
          </lu>
        </chunk>
        <var n="b"/> <!-- can't use <b/> since that would consume one of the between-words input blanks (which might be empty string) -->
      </out>
    </def-macro>

    <def-macro n="out_gen-prep" npar="1"
               c="arg.1: noun. Only acts on genitives. Adds a space after.
                  See also bigram_genprep which puts a prep-tag on the _chunk_ which may be used to change this one in t2x.">
      <let><var n="lemma"/><clip pos="1" side="tl" part="lemh"/></let>
      <modify-case><var n="lemma"/><lit v="aa"/></modify-case>
      <choose><when c="gen">
        <test><and>
          <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="gen"/></equal>
          <equal><clip pos="1" side="tl" part="a_nom"/><lit-tag v="n"/></equal>
          <!-- TODO: We might want a def-list of exceptions where we want gen-prep even if indefinite, e.g. "avsenders saksbehandler" -->
          <not><equal><clip pos="1" side="tl" part="art"/><lit-tag v="ind"/></equal></not> <!-- to bilers bredde: no gen-drop on ind -->
        </and></test>
        <choose>
          <!-- Manual lists first: -->
          <when c="i+defdrop">
            <test><in caseless="yes"><var n="lemma"/><list n="ind-gen-i"/></in></test>
            <let><clip pos="1" side="tl" part="tags"/><concat><clip pos="1" side="tl" part="tags"/><lit-tag v="defdrop"/></concat></let>
            <let><var n="gen-prep"/><lit v="i"/></let>
          </when>
          <when c="på+defdrop">
            <test><in><var n="lemma"/><list n="ind-gen-på"/></in></test>
            <let><clip pos="1" side="tl" part="tags"/><concat><clip pos="1" side="tl" part="tags"/><lit-tag v="defdrop"/></concat></let>
            <let><var n="gen-prep"/><lit v="på"/></let>
          </when>
          <when><test><in><var n="lemma"/><list n="gen-i2"/></in></test><let><var n="gen-prep"/><lit v="i"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-på2"/></in></test><let><var n="gen-prep"/><lit v="på"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-av2"/></in></test><let><var n="gen-prep"/><lit v="av"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-for2"/></in></test><let><var n="gen-prep"/><lit v="for"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-frå2"/></in></test><let><var n="gen-prep"/><lit v="frå"/></let></when>
          <when c="Some prepositions only on plurals – «blant menn» but not *«blant mannen»">
            <test><and>
              <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
              <in><var n="lemma"/><list n="gen-blant"/></in>
            </and></test>
            <let><var n="gen-prep"/><lit v="blant"/></let>
          </when>
          <when c="Some prepositions only on plurals – «mellom menn» but not *«mellom mannen»">
            <test><and>
              <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
              <in><var n="lemma"/><list n="gen-mellom"/></in>
            </and></test>
            <let><var n="gen-prep"/><lit v="mellom"/></let>
          </when>
          <!-- dev/genitive-prepositions: -->
          <when><test><in><var n="lemma"/><list n="gen-av"/></in></test><let><var n="gen-prep"/><lit v="av"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-etter"/></in></test><let><var n="gen-prep"/><lit v="etter"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-for"/></in></test><let><var n="gen-prep"/><lit v="for"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-frå"/></in></test><let><var n="gen-prep"/><lit v="frå"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-i"/></in></test><let><var n="gen-prep"/><lit v="i"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-innan"/></in></test><let><var n="gen-prep"/><lit v="innan"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-om"/></in></test><let><var n="gen-prep"/><lit v="om"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-på"/></in></test><let><var n="gen-prep"/><lit v="på"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-som"/></in></test><let><var n="gen-prep"/><lit v="som"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-ved"/></in></test><let><var n="gen-prep"/><lit v="ved"/></let></when>

          <otherwise c="til">
            <let><var n="gen-prep"/><lit v="til"/></let>
          </otherwise>
        </choose>
        <call-macro n="out_gen-prep_from_var"/>
      </when></choose>
    </def-macro>

    <def-macro n="set_gen-prep_np.org" npar="1"
               c="arg.1: proper org noun
                  Used by rules for NP.ORG.GEN NP.PERS
                  Does not output (we don't want to rewrite all these)">
      <let><var n="lemma"/><clip pos="1" side="tl" part="lemh"/></let>
      <modify-case><var n="lemma"/><lit v="aa"/></modify-case>
      <choose>
        <when><test><in><var n="lemma"/><list n="org-gen-i"/></in></test>  <let><var n="gen-prep"/><lit v="i"/></let></when>
        <when><test><in><var n="lemma"/><list n="org-gen-frå"/></in></test><let><var n="gen-prep"/><lit v="frå"/></let></when>
        <otherwise c="No genitive rewriting:">
          <let><var n="gen-prep"/><lit v=""/></let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="gen2cmp" npar="2"
               c="arg.1: genitive noun
                  arg.2: owned noun">
      <!-- TODO: rejection doesn't actually work in macros -->
      <choose><when c="avoid samfunnsverdet, regjeringsverdet, kyrkjeverdet">
        <test><and>
          <equal><clip pos="2" side="tl" part="lemh"/><lit v="verd"/></equal>
          <not><or>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="menneske"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="individ"/></equal>
          </or></not>
        </and></test>
        <reject-current-rule shifting="no"/>
      </when></choose>

      <let><var n="c_name"/><lit v="n_n"/></let>
      <call-macro n="set_caseFirstWord2"><with-param pos="1"/><with-param pos="2"/></call-macro>

      <call-macro n="stash_case_in_ntags1"><with-param pos="1"/></call-macro>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="sp"/></equal></test>
          <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sp.cmp"/></let>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
        </when>
        <otherwise>
          <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></let>
          <let><clip pos="1" side="tl" part="art"/><lit-tag v="ind.cmp"/></let>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
        </otherwise>
      </choose>
      <let><clip pos="2" side="tl" part="art"/><lit-tag v="def"/></let>
      <out>
        <chunk namefrom="c_name" case="caseFirstWord">
          <tags>
            <tag><clip pos="2" side="tl" part="tags"/></tag>
          </tags>
          <lu><clip pos="1" side="tl" part="whole"/></lu>
          <lu><clip pos="2" side="tl" part="whole"/></lu>
        </chunk>
      </out>
      <choose><when>
        <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
        <out><b pos="1"/></out>
      </when></choose>
    </def-macro>


    <def-macro n="ref-subj2adj" npar="1"
               c="arg.1: adjective, possibly with subj in ref
                  Apply subject gender/number to participle adjective (given subject stored in side ref)
                  We don't change when subject is formal 'det'.">
      <let><var n="adjtags"/><lit v=""/></let> <!-- by default, don't change anything -->
      <choose>
        <when>
          <test><and>
            <or>
              <equal><clip pos="1" side="tl" part="temps"/><lit-tag v="pp"/></equal> <!-- participles might not agree in bokmål, so here we want to guess both gender and number agreement -->
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="pst"/></equal> <!-- comparatives don't agree; superlatives do, but only in number, which will be the same in bokmål -->
            </or>
            <equal><clip pos="1" side="tl" part="a_cmp"/><lit v=""/></equal> <!-- Not a compound or split-compound -->
            <not><equal><clip pos="1" side="tl" part="a_adj"/><lit-tag v="adj.ord"/></equal></not>
            <not><equal><clip pos="1" side="tl" part="art"/><lit-tag v="def"/></equal></not> <!-- det farlige er … -->
            <not><in><clip pos="1" side="tl" part="lemh"/><list n="no-samsvar"/></in></not>
            <not><equal><clip pos="1" side="ref" part="a_expl"/><lit-tag v="expl"/></equal></not>
          </and></test>
          <choose>
            <when>
              <test><or>
                <equal><clip pos="1" side="ref" part="a_date"/><lit-tag v="date"/></equal>
                <equal><clip pos="1" side="ref" part="a_itg"/><lit-tag v="itg"/></equal>
                <equal><clip pos="1" side="ref" part="lemh"/><lit v="at"/></equal>
                <equal><clip pos="1" side="ref" part="lemh"/><lit v="km"/></equal> <!-- «15. km klassisk» (avoid «klassiske») -->
              </or></test>
              <let><var n="adjtags"/><lit-tag v="nt.sg.ind"/></let>
              <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><lit v=".date.nt)]"/></concat></let><out><var n="tmp"/></out> -->
            </when>
            <when>
              <test><or>
                <equal><clip pos="1" side="ref" part="nbr"/><lit-tag v="pl"/></equal>
                <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="tusenvis"/></ends-with>
                <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="hundrevis"/></ends-with>
                <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="mange"/></ends-with>
                <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="halvpart"/></ends-with> <!-- halvparten vart drepne -->
                <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="ingen"/></ends-with>
              </or></test>
              <choose>
                <when c="slightly different tagging of pp vs lexical adj's here">
                  <test><equal><clip pos="1" side="tl" part="temps"/><lit-tag v="pp"/></equal></test>
                  <let><var n="adjtags"/><lit-tag v="pl"/></let>
                </when>
                <otherwise>
                  <let><var n="adjtags"/><lit-tag v="un.pl.ind"/></let>
                </otherwise>
              </choose>
              <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><lit v=".pl)]"/></concat></let><out><var n="tmp"/></out> -->
            </when>
            <when>
              <test><or>
                <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="mf"/></equal>
                <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="f"/></equal>
                <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="m"/></equal>
                <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="cog"/></begins-with>
                <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="ant"/></begins-with>
                <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="org"/></begins-with>
              </or></test>
              <let><var n="adjtags"/><lit-tag v="mf.sg.ind"/></let>
              <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><lit v=".mf)]"/></concat></let><out><var n="tmp"/></out> -->
            </when>
            <when>
              <test><or>
                <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="nt"/></equal>
                <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="top"/></begins-with>
              </or></test>
              <let><var n="adjtags"/><lit-tag v="nt.sg.ind"/></let>
              <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><lit v=".nt)]"/></concat></let><out><var n="tmp"/></out> -->
            </when>
            <!-- otherwise, no good subject: leave as-is / fall back to nob.rlx disambiguation -->
          </choose>
        </when>
      </choose>
      <choose><when c="If the above tests found gender to change, set tags">
        <test><not><equal><var n="adjtags"/><lit v=""/></equal></not></test>
        <let>
          <clip pos="1" side="tl" part="tags"/>
          <concat>
            <clip pos="1" side="tl" part="a_adj"/>
            <clip pos="1" side="tl" part="grau"/>
            <var n="adjtags"/>
          </concat>
        </let>
      </when></choose>
    </def-macro>

    <def-macro n="ref-subj2pasv" npar="1"
               c="arg.1: passive, possibly with subj in ref
                  Apply subject gender/number to passive participle (given subject stored in side ref)
                  We don't change when subject is formal 'det'.">
      <choose c="Apply anaphoric variables">
        <when>
          <test><equal><clip pos="1" side="ref" part="a_date"/><lit-tag v="date"/></equal></test>
          <let><var n="pp_nbr"/><lit-tag v="sg"/></let>
          <let><var n="pp_gen"/><lit-tag v="nt"/></let>
        </when>
        <when>
          <test><and>
            <not><in><clip pos="1" side="tl" part="lemh"/><list n="no-samsvar"/></in></not>
            <not><equal><clip pos="1" side="ref" part="a_expl"/><lit-tag v="expl"/></equal></not>
            <or>
              <equal><clip pos="1" side="ref" part="nbr"/><lit-tag v="pl"/></equal>
              <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="tusenvis"/></ends-with>
              <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="hundrevis"/></ends-with>
              <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="mange"/></ends-with>
              <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="halvpart"/></ends-with> <!-- halvparten vart drepne -->
              <ends-with><clip pos="1" side="ref" part="lemh"/><lit v="ingen"/></ends-with>
            </or>
          </and></test>
          <let><var n="pp_nbr"/><lit-tag v="pl"/></let>
          <let><var n="pp_gen"/><lit-tag v="un"/></let> <!-- just some nonempty value we can override the slot in t2x -->
          <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><var n="pp_nbr"/><lit v=")]"/></concat></let><out><var n="tmp"/></out> -->
        </when>
        <otherwise>
          <let><var n="pp_nbr"/><lit-tag v="sg"/></let>
          <let><var n="pp_gen"/><lit-tag v="nt"/></let>
          <choose><when>
            <test><or>
              <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="mf"/></equal>
              <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="f"/></equal>
              <equal><clip pos="1" side="ref" part="gen"/><lit-tag v="m"/></equal>
              <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="cog"/></begins-with>
              <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="ant"/></begins-with>
              <begins-with><clip pos="1" side="ref" part="a_prop"/><lit-tag v="org"/></begins-with>
            </or></test>
            <let><var n="pp_gen"/><lit-tag v="mf"/></let>
          </when></choose>
          <!-- <let><var n="tmp"/><concat><lit v="[("/><clip pos="1" side="ref" part="lemh"/><var n="pp_gen"/><lit v=")]"/></concat></let><out><var n="tmp"/></out> -->
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="out_verb" npar="1"
               c="arg.1: verb, passive or active, possibly compound-right
                  Assumes these variables have been set first:
                  - c_name
                  - caseFirstWord
                  - cmp-lefts">
      <call-macro n="flush-mwe-queue"><with-param pos="1"/></call-macro>
      <call-macro n="set_lemq"><with-param pos="1"/></call-macro>
      <choose>
        <when c="Run aux + pp transformation on passives that are finite or immediately follow 'å'">
          <test><and>
            <equal><clip pos="1" side="tl" part="voice"/><lit-tag v="pasv"/></equal>
            <or>
              <equal><var n="prevlem"/><lit v="å"/></equal>
              <equal><clip pos="1" side="tl" part="temps"/><lit-tag v="pres"/></equal>
              <equal><clip pos="1" side="tl" part="temps"/><lit-tag v="pret"/></equal>
            </or>
          </and></test>
          <call-macro n="set_aux"><with-param pos="1"/></call-macro> <!-- TODO: should use case from cmp -->
          <choose c="Only movable past an NP if no lemq or not in np-immovable-verb list, and refsyn has found subject after verb">
            <when>
              <test><or>
                <not><equal><clip pos="1" side="ref" part="a_postsubj"/><lit-tag v="postsubj"/></equal></not>
                <in><clip pos="1" side="tl" part="lemh"/><list n="np-immovable-verb"/></in>
                <not><equal><clip pos="1" side="tl" part="lemq"/><lit v=""/></equal></not>
              </or></test>
              <let><var n="movable_tag"/><lit v=""/></let>
            </when>
            <otherwise><let><var n="movable_tag"/><lit-tag v="np-movable"/></let></otherwise>
          </choose>
          <call-macro n="ref-subj2pasv"><with-param pos="1"/></call-macro>
          <append n="c_name"><lit v="adj2vblex"/></append>
          <out>
            <chunk name="aux" case="caseFirstWord">
              <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
              <var n="aux"/>
            </chunk>
            <b/>
            <chunk namefrom="c_name">
              <tags>
                <tag><lit-tag v="adj.pp"/></tag>
                <tag><var n="pp_gen"/></tag>
                <tag><var n="pp_nbr"/></tag>
                <tag><lit-tag v="ind"/></tag>
                <tag><var n="movable_tag"/></tag>
              </tags>
              <var n="cmp-lefts"/>
              <lu>
                <clip pos="1" side="tl" part="lemh"/>
                <lit-tag v="adj.pp"/>
                <var n="pp_gen"/>
                <var n="pp_nbr"/>
                <lit-tag v="ind"/>
                <var n="c_lemq"/>
              </lu>
            </chunk>
          </out>
        </when>
        <otherwise c="Active or infinitive passive after modal – no transformation">
          <choose c="Set c_name">
            <when>
              <test><and>
                <equal><var n="c_name"/><lit v=""/></equal> <!-- not a compound -->
                <or>
                  <equal><clip pos="1" side="tl" part="lemh"/><lit v="bli"/></equal>
                  <equal><clip pos="1" side="tl" part="lemh"/><lit v="vere"/></equal>
                  <equal><clip pos="1" side="tl" part="lemh"/><lit v="ha"/></equal>
                </or>
              </and></test>
              <let><var n="c_name"/><lit v="aux"/></let>
            </when>
            <otherwise>
              <append n="c_name"><lit v="vblex"/></append>
            </otherwise>
          </choose>
          <out>
            <chunk namefrom="c_name" case="caseFirstWord">
              <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
              <var n="cmp-lefts"/>
              <lu>
                <clip pos="1" side="tl" part="lemh"/>
                <clip pos="1" side="tl" part="a_verb"/>
                <clip pos="1" side="tl" part="temps"/>
                <clip pos="1" side="tl" part="voice"/>
                <clip pos="1" side="tl" part="a_cmp"/>
                <var n="c_lemq"/>
              </lu>
            </chunk>
          </out>
        </otherwise>
      </choose>
      <out><var n="out_lemq"/></out>
    </def-macro>

  </section-def-macros>



  <section-rules>

    <!-- NP.ORG.GEN rules -->

    <rule comment="NP.ORG.GEN NP.PERS → NP.PERS i NP.ORG
                   SVs Rasmus Berg → Rasmus Berg i SV">
