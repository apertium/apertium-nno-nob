            <lu><clip pos="2" side="tl" part="whole"/></lu> <!-- {^finanspolitisk<adj><pst><un><sp><def>$ ^talsperson<n><m><sg><ind>$}$  -->
            <b/>
            <lu><clip pos="3" side="tl" part="whole"/></lu>

          </chunk>
          <b/>
          <chunk name="pr">
            <tags><tag><lit-tag v="pr"/></tag></tags>
            <lu><lit v="i"/><lit-tag v="pr"/></lu>
          </chunk>
          <b/>
          <chunk name="nn">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
