        <pattern-item n="remspc"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <let><clip pos="3" side="tl" part="a_remspc"/><lit v=""/></let>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <chunk name="sent">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- idiomatic NGEN NIND → (DET) ADJ NDEF -->
    <rule comment="britenes statsminister → (den) britiske statsministeren">
      <pattern>
        <pattern-item n="britenes"/>
        <pattern-item n="nind_indgen"/>
      </pattern>
      <action>
        <let><var n="brlemma"/><clip pos="1" side="sl" part="lemh"/></let>
        <modify-case><var n="brlemma"/><lit v="aa"/></modify-case>
        <choose>
          <when>
            <test><equal><var n="brlemma"/><lit v="brite"/></equal></test>
            <let>        <var n="brlemma"/><lit v="britisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="russer"/></equal></test>
            <let>        <var n="brlemma"/><lit v="russisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="sveitser"/></equal></test>
            <let>        <var n="brlemma"/><lit v="sveitsisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="same"/></equal></test>
            <let>        <var n="brlemma"/><lit v="samisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="india"/></equal></test>
            <let>        <var n="brlemma"/><lit v="indisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="kina"/></equal></test>
            <let>        <var n="brlemma"/><lit v="kinesisk"/></let>
          </when>
          <otherwise>
            <reject-current-rule shifting="no"/>
          </otherwise>
        </choose>
        <choose>
          <when>
            <test><or>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="side"/></equal>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="utmelding"/></equal>
            </or></test>
            <let><var n="c_name"/><lit v="adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.mf.sg.ind"/></let>
            <let><var n="defnes"/><lit-tag v="ind"/></let>
          </when>
          <otherwise>
            <let><var n="c_name"/><lit v="needdet_adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.un.sp.def"/></let>
            <let><var n="defnes"/><lit-tag v="def"/></let>
          </otherwise>
        </choose>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <let><clip pos="2" side="tl" part="art"/><var n="defnes"/></let>
        <call-macro n="stash_case_in_ntags1"><with-param pos="2"/></call-macro>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <var n="grau_aux"/>
            <lu>
              <var n="brlemma"/>
              <var n="adjtags"/>
            </lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- Verbs -->
    <rule comment="VBLEX.PASV.FV: leses => vert lest
                   beholdes [ikke] => vert [ikkje] haldt på
                   synes => synest

                   TODO: verb probably should lose possible uppercasing,
                   although it's unlikely to find the passive verb in
                   the beginning of a sentence

                   If we translate into a pstv verb, output like the regular VBLEX rule.

                   If we don't see an infinitive marker in t2x, the aux gets
                   deleted again.">
      <pattern>
        <pattern-item n="vblexpassfv"/>
      </pattern>
      <action>
        <choose>
          <when c="Only output if bidix has a lemma in l">
            <test>
              <not><equal><clip pos="1" side="tl" part="lem"/><lit v=""/></equal></not>
            </test>
            <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
            <call-macro n="set_lemq"><with-param pos="1"/></call-macro>

            <choose>
              <when>
                <test><or>
                  <equal><clip pos="1" side="tl" part="a_verb"/><lit-tag v="vblex.pstv"/></equal>
                  <equal><clip pos="1" side="sl" part="a_verb"/><lit-tag v="vblex.pstv"/></equal>
                  <equal><clip pos="1" side="tl" part="voice"/><lit v=""/></equal> <!-- explicitly translating into active form in bidix -->
                </or></test>
                <out>
                  <chunk name="vblex" case="caseFirstWord">
                    <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                    <lu>
                      <clip pos="1" side="tl" part="lemh"/>
                      <clip pos="1" side="tl" part="a_verb"/>
                      <clip pos="1" side="tl" part="temps"/>
                      <var n="c_lemq"/>
                    </lu>
                  </chunk>
                </out>
              </when>
              <otherwise>
                <call-macro n="set_aux"><with-param pos="1"/></call-macro>
                <!-- todo: why do I get a bus error when calling set_aux2 here? -->
                <choose c="Only movable past an NP if no lemq or not in np-immovable-verb list">
                  <when>
                    <test><or>
                      <in><clip pos="1" side="tl" part="lemh"/><list n="np-immovable-verb"/></in>
                      <not><equal><clip pos="1" side="tl" part="lemq"/><lit v=""/></equal></not>
                    </or></test>
                    <let><var n="movable_tag"/><lit v=""/></let>
                  </when>
                  <otherwise><let><var n="movable_tag"/><lit-tag v="np-movable"/></let></otherwise>
                </choose>
                <out>
                  <chunk name="aux" case="caseFirstWord">
                    <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                    <var n="aux"/>
                  </chunk>
                  <b/>
                  <chunk name="adj2vblex">
                    <tags>
                      <tag><lit-tag v="adj.pp.nt.sg.ind"/></tag>
                      <tag><var n="movable_tag"/></tag>
                    </tags>
                    <lu>
                      <clip pos="1" side="tl" part="lemh"/>
                      <lit-tag v="adj.pp.nt.sg.ind"/>
                      <var n="c_lemq"/>
                    </lu>
                  </chunk>
                </out>
              </otherwise>
            </choose>
            <out><var n="out_lemq"/></out>
          </when>
          <otherwise>
            <let><var n="prevDeletedCase"/><lit v="aa"/></let>
            <modify-case><var n="prevDeletedCase"/><var n="caseFirstWord"/></modify-case>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="VBLEXPP
                   This rule has to be above the regular vblex rule,
                   override lemq-moving for pp verbs.">
      <pattern>
        <pattern-item n="vblexpp"/>
      </pattern>
      <action>
        <choose>
          <when c="Only output if bidix has a lemma in l">
            <test>
              <not><equal><clip pos="1" side="tl" part="lem"/><lit v=""/></equal></not>
            </test>
            <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
            <let><var n="c_name"/><lit v="vblex"/></let>
            <choose><when>
              <test><or>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="bli"/></equal>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="vere"/></equal>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="ha"/></equal>
              </or></test>
              <let><var n="c_name"/><lit v="aux"/></let>
            </when></choose>
            <out>
              <chunk namefrom="c_name" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </lu>
              </chunk>
            </out>
          </when>
          <otherwise>
            <let><var n="prevDeletedCase"/><lit v="aa"/></let>
            <modify-case><var n="prevDeletedCase"/><var n="caseFirstWord"/></modify-case>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="VBLEX
                   If we have nn multiwords, these need the lemq after the
                   inflected part. All lemq's include their own initial space.
                   This rule has to be below the passive rule.">
      <pattern>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="flush-mwe-queue"><with-param pos="1"/></call-macro>
        <choose>
          <when c="Only output if bidix has a lemma in l">
            <test>
              <not><equal><clip pos="1" side="tl" part="lem"/><lit v=""/></equal></not>
            </test>
            <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
            <call-macro n="set_lemq"><with-param pos="1"/></call-macro>
            <let><var n="c_name"/><lit v="vblex"/></let>
            <choose><when>
              <test><or>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="bli"/></equal>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="vere"/></equal>
                <equal><clip pos="1" side="tl" part="lemh"/><lit v="ha"/></equal>
              </or></test>
              <let><var n="c_name"/><lit v="aux"/></let>
            </when></choose>
            <out>
              <chunk namefrom="c_name" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <clip pos="1" side="tl" part="voice"/>
                  <var n="c_lemq"/>
                </lu>
              </chunk>
            </out>
            <out><var n="out_lemq"/></out>
          </when>
          <otherwise>
            <let><var n="prevDeletedCase"/><lit v="aa"/></let>
            <modify-case><var n="prevDeletedCase"/><var n="caseFirstWord"/></modify-case>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="INFM">
      <pattern>
        <pattern-item n="infm"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="infm" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="tl" part="tags"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="ADV">
      <pattern>
        <pattern-item n="adv"/>
      </pattern>
      <action><choose><when c="do nothing if empty target lemma">
        <test><not><equal><clip pos="1" side="tl" part="lemh"/><lit v=""/></equal></not></test>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let><var n="advtags"/><clip pos="1" side="tl" part="tags"/></let>
        <choose><when>
          <test><not><in><clip pos="1" side="tl" part="lemh"/><list n="immovable-adv"/></in></not></test>
          <append n="advtags">
            <lit-tag v="movable"/>
          </append>
        </when></choose>
        <choose><when>
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="n-movable-adv"/></in></test>
          <append n="advtags">
            <lit-tag v="n-movable"/>
          </append>
        </when></choose>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><var n="advtags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      </when></choose></action>
    </rule>
    <rule comment="så-ADV lenge-ADV/ADJ: så lenge → så lenge">
      <pattern>
        <pattern-item n="så-adv"/>
        <pattern-item n="lenge"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <let><var n="advtags"/><clip pos="1" side="tl" part="tags"/></let>
        <!-- not movable -->
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><var n="advtags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="2" side="tl" part="lem"/></let>
      </action>
    </rule>

    <rule comment="akkurat DETPOS: akkurat deres by → akkurat deira by
                   Tell t2x to not use DETPOS NIND → NDEF DETPOS rule.">
      <pattern>
        <pattern-item n="akkurat"/>
        <pattern-item n="detpos"/>
        <pattern-item n="n_np"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <choose>
          <when>
            <test><and>
              <or>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="m"/></equal>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="f"/></equal>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="nt"/></equal>
              </or>
              <not><equal><clip pos="3" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
              <not><equal><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
            </and></test>
            <let><clip pos="2" side="tl" part="gen"/><clip pos="3" side="tl" part="gen"/></let>
          </when>
        </choose>
        <out>
          <chunk name="adv">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="detpos-keep">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="2"/>
          <chunk name="nn">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="DETPOS ADJSUP_EMPH_ORD NIND: sin beste form → sin beste form
                                            min kjære undulat → min kjære undulat
                   Tell t2x to not use DETPOS NIND → NDEF DETPOS rule.">
      <pattern>
        <pattern-item n="detpos"/>
        <pattern-item n="adjsup_emph_ord"/>
        <pattern-item n="nind_indgen"/>
      </pattern>
      <action>
        <choose><when>
          <test><equal><clip pos="1" side="tl" part="lemh"/><lit v=""/></equal></test>
          <reject-current-rule shifting="no"/>
        </when></choose>
        <choose>
          <when>
            <test><and>
              <or>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="m"/></equal>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="f"/></equal>
                <equal><clip pos="3" side="tl" part="gen"/><lit-tag v="nt"/></equal>
              </or>
              <not><equal><clip pos="3" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
              <not><equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
            </and></test>
            <let><clip pos="1" side="tl" part="gen"/><clip pos="3" side="tl" part="gen"/></let>
          </when>
        </choose>
        <let><var n="c_name"/><lit v="adj_n"/></let>
        <call-macro n="set_gender2.n.adj"><with-param pos="3"/><with-param pos="2"/></call-macro>
        <call-macro n="set_grau_aux1"><with-param pos="2"/></call-macro>
        <call-macro n="set_caseFirstWord3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <out>
          <chunk name="detpos-keep" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk namefrom="c_name" case="caseSecondWord">
            <tags>
              <tag><clip pos="3" side="tl" part="tags"/></tag>
            </tags>
            <var n="grau_aux"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="tags"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
            <b pos="2"/>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="DETPOS ADJSUP_EMPH_ORD ADJ NIND: sin andre gode fangst → sin andre gode fangst
                   Tell t2x to not use DETPOS NIND → NDEF DETPOS rule.">
      <pattern>
        <pattern-item n="detpos"/>
        <pattern-item n="adjsup_emph_ord"/>
        <pattern-item n="adj"/>
        <pattern-item n="nind_indgen"/>
      </pattern>
      <action>
        <choose><when>
          <test><equal><clip pos="1" side="tl" part="lemh"/><lit v=""/></equal></test>
          <reject-current-rule shifting="no"/>
        </when></choose>
        <choose>
          <when>
            <test><and>
              <or>
                <equal><clip pos="4" side="tl" part="gen"/><lit-tag v="m"/></equal>
                <equal><clip pos="4" side="tl" part="gen"/><lit-tag v="f"/></equal>
                <equal><clip pos="4" side="tl" part="gen"/><lit-tag v="nt"/></equal>
              </or>
              <not><equal><clip pos="4" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
              <not><equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal></not>
            </and></test>
            <let><clip pos="1" side="tl" part="gen"/><clip pos="4" side="tl" part="gen"/></let>
          </when>
        </choose>
        <let><var n="c_name"/><lit v="adj_adj_n"/></let>
        <call-macro n="set_gender2.n.adj"><with-param pos="4"/><with-param pos="3"/></call-macro>
        <call-macro n="set_grau_aux1"><with-param pos="3"/></call-macro>
        <call-macro n="set_caseFirstWord4">
          <with-param pos="1"/>
          <with-param pos="2"/>
          <with-param pos="3"/>
          <with-param pos="4"/>
        </call-macro>
        <out>
          <chunk name="detpos-keep" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
          <b/>
          <chunk namefrom="c_name" case="caseSecondWord">
            <tags>
              <tag><clip pos="4" side="tl" part="tags"/></tag>
            </tags>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="tags"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
            <b/>
            <var n="grau_aux"/>
            <lu>
              <clip pos="3" side="tl" part="lemh"/>
              <clip pos="3" side="tl" part="tags"/>
              <clip pos="3" side="tl" part="lemq"/>
            </lu>
            <b/>
            <lu><clip pos="4" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- POSGEN måte å VBLEXINF -->
    <rule comment="sin måte å VBLEXINF: sin måte å referere => sin måte å referere
                   Override the below rule (TODO: use exception tag instead, when accepted to trunk).">
      <pattern>
        <pattern-item n="sin"/>
        <pattern-item n="måte"/>
        <pattern-item n="infm"/>
        <pattern-item n="vblexinf"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord4">
          <with-param pos="1"/>
          <with-param pos="2"/>
          <with-param pos="3"/>
          <with-param pos="4"/>
        </call-macro>
        <call-macro n="set_lemq"><with-param pos="4"/></call-macro>
        <out>
          <chunk name="detsin">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="nn">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="2"/>
          <chunk name="infm">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="3"/>
        </out>
        <let><var n="c_name"/><lit v="vblex"/></let>
        <choose><when>
          <test><or>
            <equal><clip pos="4" side="tl" part="lemh"/><lit v="bli"/></equal>
            <equal><clip pos="4" side="tl" part="lemh"/><lit v="vere"/></equal>
          </or></test>
          <let><var n="c_name"/><lit v="aux"/></let>
        </when></choose>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><clip pos="4" side="tl" part="tags"/></tag></tags>
            <lu>
              <clip pos="4" side="tl" part="lemh"/>
              <clip pos="4" side="tl" part="a_verb"/>
              <clip pos="4" side="tl" part="temps"/>
              <var n="c_lemq"/>
            </lu>
          </chunk>
        </out>
        <out><var n="out_lemq"/></out>
      </action>
    </rule>

    <rule comment="POSGEN måte å VBLEXINF: hennes måte å sykle => måten ho syklar
                   bokens måte å leses => måten boka vert lesen

Requires rather ugly RL prn lemmas in nn.dix, see personal_pronouns__prn pardef

no lemq-moving">
      <pattern>
        <pattern-item n="posgen"/>
        <pattern-item n="måte"/>
        <pattern-item n="infm"/>
        <pattern-item n="vblexinf_or_infpass"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord4">
          <with-param pos="1"/>
          <with-param pos="2"/>
          <with-param pos="3"/>
          <with-param pos="4"/>
        </call-macro>
        <call-macro n="out_ndef">
	  <with-param pos="2"/>
	  <with-param pos="1"/>
	</call-macro>
        <out><b pos="1"/></out>
        <call-macro n="out_posgen_subj">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose><when>
          <test><not><equal><b pos="2"/><lit v=" "/></equal></not></test>
          <out><b pos="2"/></out>
        </when></choose>
        <choose>
          <when>
            <test>
              <equal><clip pos="4" side="tl" part="voice"/><lit-tag v="pasv"/></equal>
            </test>
            <call-macro n="set_aux2"><with-param pos="4"/><with-param pos="4"/></call-macro>
            <out>
              <chunk name="vblex" case="caseFirstWord">
                <tags><tag><clip pos="4" side="tl" part="tags"/></tag></tags>
                <var n="aux"/>
              </chunk>
              <b pos="3"/>
              <chunk name="vblex">
                <tags><tag><clip pos="4" side="tl" part="tags"/></tag></tags>
                <lu>
                  <clip pos="4" side="tl" part="lemh"/>
                  <clip pos="4" side="tl" part="a_verb"/>
                  <lit-tag v="pp"/>
                  <clip pos="4" side="tl" part="lemq"/>
                </lu>
              </chunk>
            </out>
          </when>
          <otherwise>
            <out>
              <b pos="3"/>
              <chunk name="vblex" case="caseFirstWord">
                <tags><tag><clip pos="4" side="tl" part="tags"/></tag></tags>
                <lu>
                  <clip pos="4" side="tl" part="lemh"/>
                  <clip pos="4" side="tl" part="a_verb"/>
                  <lit-tag v="pres"/>
                  <clip pos="4" side="tl" part="lemq"/>
                </lu>
              </chunk>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <!-- Catch wrong bracketing:  -->
    <rule comment="preadj-ADV ADJIND
