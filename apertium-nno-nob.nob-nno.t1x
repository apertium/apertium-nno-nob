    <rule comment="DET">
      <pattern>
        <pattern-item n="det"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let><var n="c_name"/><lit v="det"/></let>
        <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
        <choose c="set c_name">
          <when>
            <test><equal><clip pos="1" side="tl" part="expl"/><lit-tag v="expl"/></equal></test>
            <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
            <let><var n="c_name"/><lit v="detexpl"/></let>
          </when>
          <when>
            <test><in caseless="yes"><clip pos="1" side="sl" part="lem"/><list n="det_indef"/></in></test>
            <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
            <let><var n="c_name"/><lit v="detdd"/></let>
          </when>
          <when>
            <test><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="sin"/></equal></test>
            <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
            <let><var n="c_name"/><lit v="detsin"/></let>
          </when>
          <when>
            <test><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="all"/></equal></test>
            <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
            <let><var n="c_name"/><lit v="detall"/></let>
            <choose><when>
              <test><equal caseless="yes"><clip pos="1" side="sl" part="lemh"/><lit v="enhver"/></equal></test>
              <let><var n="c_name"/><lit v="detenhver"/></let>
              <let><clip pos="1" side="tl" part="gen"/><lit-tag v="un"/></let>
              <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></let>
              <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
            </when></choose>
            <choose><when>
              <test><equal caseless="yes"><clip pos="1" side="sl" part="lemh"/><lit v="samtlige"/></equal></test>
              <let><var n="c_name"/><lit v="detsamtlige"/></let>
              <let><var n="dettags"/><clip pos="1" side="tl" part="tags"/></let>
              <let><clip pos="1" side="tl" part="art"/><lit v=""/></let>
            </when></choose>
          </when>
        </choose>

        <choose><when c="There are no genitive det's in nn.dix, output prep (most likely changed to 'sin' in t2x)">
          <test><equal><clip pos="1" side="tl" part="cas"/><lit-tag v="gen"/></equal></test>
          <call-macro n="out_gen-prep"><with-param pos="1"/></call-macro>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
        </when></choose>

        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><var n="dettags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

