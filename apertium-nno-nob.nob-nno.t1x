          <out><b pos="1"/></out>
        </when></choose>
        <let>
          <var n="mwe-queue"/>
          <chunk name="default">
            <tags><tag><lit-tag v="default"/></tag></tags>
            <lu>
              <lit v="i"/>
              <lit-tag v="pr"/>
            </lu>
            <b/>
            <lu>
