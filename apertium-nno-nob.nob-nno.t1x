    <rule comment="PR">
      <pattern>
        <pattern-item n="pr"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let>
          <var n="c_lemtag"/>
          <concat><lit v="&lt;"/><clip pos="1" side="tl" part="lemh"/><lit v="&gt;"/></concat>
        </let>
        <choose>
          <when>
            <test><or>
              <and><in caseless="yes"><var n="prevlem"/><list n="noun_takes_i"/></in>  <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="i"/></equal></and>
              <and><in caseless="yes"><var n="prevlem"/><list n="noun_takes_til"/></in><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="til"/></equal></and>
            </or></test>
            <let><var n="pr_head"/><lit-tag v="nhead"/></let>
          </when>
        <otherwise>
          <!-- By default no tag, assume verb head: -->
          <let><var n="pr_head"/><lit v=""/></let>
        </otherwise>
        </choose>
        <out>
          <chunk name="pr" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="tl" part="tags"/></tag>
              <tag><var n="c_lemtag"/></tag>
              <tag><var n="pr_head"/></tag>
            </tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
        <let><var n="prevnbr"/><clip pos="1" side="tl" part="nbr"/></let>
      </action>
    </rule>

