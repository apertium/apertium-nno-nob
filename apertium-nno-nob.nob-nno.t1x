        <pattern-item n="personnamn"/> <!-- 2 -->
      </pattern>
      <action>
        <call-macro n="set_gen-prep_np.org"><with-param pos="1"/></call-macro>
        <choose><when c="Skip if name is gen/cmp – or if not in in org-gen-i etc. lists">
          <test><or>
            <equal><var n="gen-prep"/><lit v=""/></equal>
            <equal><clip pos="2" side="sl" part="cas"/><lit-tag v="gen"/></equal>
            <not><equal><clip pos="2" side="sl" part="a_cmp"/><lit v=""/></equal></not>
          </or></test>
          <reject-current-rule shifting="no"/>
