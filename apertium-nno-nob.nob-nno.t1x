              <when><test><in><var n="lemma"/><list n="obj-gen-over"/></in></test><append n="ntags"><lit-tag v="pr_over"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-på2"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-på"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-under"/></in></test><append n="ntags"><lit-tag v="pr_under"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-ved"/></in></test><append n="ntags"><lit-tag v="pr_ved"/></append></when>
            </choose>
          </when></choose>
        </otherwise>
      </choose>
      <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      <let><var n="prevnbr"/><clip pos="1" side="tl" part="nbr"/></let>
    </def-macro>

    <def-macro n="stash_case_in_ntags0" npar="1"
               c="Helper for stash_case_in_ntags1 and stash_case_in_ntags2
                  Initialises ntags to tags; adds casedrop to ntags, clears defdrop from lu tags, adds source gender to ntags
                  Does not add keep-sin nor call bigram_genprep (we expect that to happen after call to this)
                  Empties a_caps and a_defdrop!">
      <let><clip pos="1" side="tl" part="a_caps"/><lit v=""/></let>
      <let><var n="ntags"/><clip pos="1" side="tl" part="tags"/></let>
      <choose><when>
        <test><equal><clip pos="1" side="tl" part="a_nom"/><lit-tag v="n"/></equal></test>
        <choose><when>
          <test><and>
            <not><equal><clip pos="1" side="tl" part="cas"/><lit v=""/></equal></not>
            <not><equal><clip pos="1" side="tl" part="art"/><lit-tag v="ind"/></equal></not> <!-- to bilers bredde: no gen-drop on ind -->
          </and></test>
          <append n="ntags"><lit-tag v="casedrop"/></append>
        </when></choose>
        <choose><when>
          <test><not><equal><clip pos="1" side="tl" part="a_defdrop"/><lit v=""/></equal></not></test>
          <let><clip pos="1" side="tl" part="a_defdrop"/><lit v=""/></let>
        </when></choose>
        <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="nt"/></equal></test>
            <append n="ntags"><lit-tag v="sl-nt"/></append>
          </when>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="m"/></equal></test>
            <append n="ntags"><lit-tag v="sl-m"/></append>
          </when>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="f"/></equal></test>
            <append n="ntags"><lit-tag v="sl-f"/></append>
          </when>
        </choose>
      </when></choose>
      <choose>
          <when>
            <test><and>
              <!-- refsyn.t1x strips @subj tag from sl so we don't have to change every single
                   def-cat, but if the lemma is the same as ref we're probably on the subject: -->
              <equal><clip pos="1" side="sl" part="lem"/><clip pos="1" side="ref" part="lem"/></equal>
              <or>
                <equal><clip pos="1" side="ref" part="a_syntax"/><lit-tag v="@subj"/></equal>
                <equal><clip pos="1" side="ref" part="a_syntax"/><lit-tag v="@xubj"/></equal>
              </or>
            </and></test>
            <append n="ntags"><clip pos="1" side="ref" part="a_syntax"/></append>
          </when>
        </choose>
    </def-macro>

    <def-macro n="stash_case_in_ntags1" npar="1"
               c="arg.1: noun. Puts tags of noun in ntags (for use as chunk tags).">
      <call-macro n="stash_case_in_ntags0"><with-param pos="1"/></call-macro>
      <choose><when>
        <test><in caseless="yes"><clip pos="1" side="sl" part="lemh"/><list n="n_keep_sin"/></in></test>
        <append n="ntags"><lit-tag v="keep-sin"/></append>
      </when></choose>
      <call-macro n="bigram_genprep"><with-param pos="1"/></call-macro>
    </def-macro>

    <def-macro n="stash_case_in_ntags2" npar="2"
               c="arg.1: head noun. Puts tags of noun in ntags (for use as chunk tags).
                  arg.2: coordinated noun (e.g. barndom og oppvekst), for keep-sin">
      <call-macro n="stash_case_in_ntags0"><with-param pos="1"/></call-macro>
      <choose><when>
        <test><in caseless="yes"><clip pos="1" side="sl" part="lemh"/><list n="n_keep_sin"/></in></test>
        <append n="ntags"><lit-tag v="keep-sin"/></append>
      </when>
      <when>
        <test><in caseless="yes"><clip pos="2" side="sl" part="lemh"/><list n="n_keep_sin"/></in></test>
        <append n="ntags"><lit-tag v="keep-sin"/></append>
      </when></choose>
      <call-macro n="bigram_genprep"><with-param pos="1"/></call-macro>
    </def-macro>

    <!-- Should not be used directly, see helpers below (set_caseFirst/SecondWord etc) -->
    <def-macro n="set_chunkCaseCmp" npar="2"
               c="arg1: first cmp-part, arg2: last cmp-part
                  Use nom tags from first part, caps tags from last
                  Reads chunkCase to see if we're the first word of sentence.
                  Writes to chunkCase.
                  May UPPERCASE if a_case was AA.">
      <choose>
        <when c="Use lemma case when upper (even when first in sentence)?">
          <test><and>
            <in><clip pos="2" side="tl" part="lem"/><list n="keep_lemma_case_upper"/></in>
            <equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="AA"/></equal>
          </and></test>
          <let><clip pos="2" side="sl" part="a_caps"/><lit-tag v="Aa"/></let>
        </when>
        <when c="Use lemma case when not upper (and not first in sentence)?">
          <test><and>
            <in><clip pos="2" side="tl" part="lem"/><list n="keep_lemma_case"/></in>
            <not><equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="AA"/></equal></not>
            <not><equal><var n="chunkCase"/><lit v=""/></equal></not>
          </and></test>
          <let><clip pos="2" side="sl" part="a_caps"/><lit-tag v="aa"/></let>
        </when>
      </choose>

      <let><var n="upperMiddleParts"/><lit v="no"/></let>
      <choose>
        <when c="Previous word was deleted – use its case if it was Titled and we're lower">
          <test><and>
            <equal><var n="prevDeletedCase"/><lit v="Aa"/></equal>
            <equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="aa"/></equal>
          </and></test>
          <let><var n="chunkCase"/><var n="prevDeletedCase"/></let>
          <let><var n="prevDeletedCase"/><lit v=""/></let>
        </when>
        <when c="Titlecased, but sl lemma is lowercase: set chunkCase Aa
                 Note: this will *not* match where bidix translated title to lower.
                 We use the first cmp-part since that's the part that postchunk titlecases.">
          <test><and>
            <equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="Aa"/></equal>
            <equal><get-case-from pos="1"><lit v="aa"/></get-case-from><lit v="aa"/></equal>
          </and></test>
          <let><var n="chunkCase"/><lit v="Aa"/></let>
        </when>
        <when c="UPPERCASED: set chunkCase aa, uppercase the argument">
          <test><equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="AA"/></equal></test>
          <let><var n="chunkCase"/><lit v="aa"/></let>
          <let><var n="upperMiddleParts"/><lit v="yes"/></let>
          <modify-case><clip pos="1" side="tl" part="lem"/><lit v="AA"/></modify-case>
          <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
        </when>
        <when c="Not first word in sentence: set chunkCase aa">
          <test><not><equal><var n="chunkCase"/><lit v=""/></equal></not></test>
          <let><var n="chunkCase"/><lit v="aa"/></let>
        </when>
        <when c="Titlecased, presumably first word in sentence?">
          <test><equal><clip pos="2" side="sl" part="a_caps"/><lit-tag v="Aa"/></equal></test>
          <let><var n="chunkCase"/><lit v="Aa"/></let>
        </when>
        <otherwise><let><var n="chunkCase"/><lit v="aa"/></let></otherwise>
      </choose>
      <let><var n="prevDeletedCase"/><lit v=""/></let> <!-- Ensure we don't use this more than once! -->
      <let><clip pos="1" side="tl" part="a_caps"/><lit v=""/></let> <!-- discard value -->
      <let><clip pos="2" side="tl" part="a_caps"/><lit v=""/></let> <!-- discard value -->
    </def-macro>


    <!-- These are used for calling set_chunkCaseCmp from the rules,
         depending on how many words are matched and whether they're
         compounds and could be first in sentence: -->
    <def-macro n="set_caseFirstWord" npar="1">
      <let><var n="chunkCase"/><var n="caseFirstWord"/></let>
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="1"/></call-macro>
      <let><var n="caseFirstWord"/><var n="chunkCase"/></let>
    </def-macro>
    <def-macro n="set_caseFirstWordCmp" npar="2">
      <let><var n="chunkCase"/><var n="caseFirstWord"/></let>
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="2"/></call-macro>
      <let><var n="caseFirstWord"/><var n="chunkCase"/></let>
    </def-macro>
    <def-macro n="set_caseFirstWordCmp3" npar="3">
      <let><var n="chunkCase"/><var n="caseFirstWord"/></let>
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="3"/></call-macro>
      <let><var n="caseFirstWord"/><var n="chunkCase"/></let>
      <choose><when>
        <test><equal><var n="upperMiddleParts"/><lit v="yes"/></equal></test>
        <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
      </when></choose>
    </def-macro>
    <def-macro n="set_caseFirstWordCmp4" npar="4">
      <let><var n="chunkCase"/><var n="caseFirstWord"/></let>
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="4"/></call-macro>
      <let><var n="caseFirstWord"/><var n="chunkCase"/></let>
      <choose><when>
        <test><equal><var n="upperMiddleParts"/><lit v="yes"/></equal></test>
        <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
        <modify-case><clip pos="3" side="tl" part="lem"/><lit v="AA"/></modify-case>
      </when></choose>
    </def-macro>
    <def-macro n="set_caseFirstWordCmp5" npar="5">
      <let><var n="chunkCase"/><var n="caseFirstWord"/></let>
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="5"/></call-macro>
      <let><var n="caseFirstWord"/><var n="chunkCase"/></let>
      <choose><when>
        <test><equal><var n="upperMiddleParts"/><lit v="yes"/></equal></test>
        <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
        <modify-case><clip pos="3" side="tl" part="lem"/><lit v="AA"/></modify-case>
        <modify-case><clip pos="4" side="tl" part="lem"/><lit v="AA"/></modify-case>
      </when></choose>
    </def-macro>

    <def-macro n="set_caseSecondWord" npar="1">
      <let><var n="chunkCase"/><lit v="aa"/></let> <!-- ie. we're not the first word of sentence -->
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="1"/></call-macro>
      <let><var n="caseSecondWord"/><var n="chunkCase"/></let>
    </def-macro>
    <def-macro n="set_caseSecondWordCmp" npar="2">
      <let><var n="chunkCase"/><lit v="aa"/></let> <!-- ie. we're not the first word of sentence -->
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="2"/></call-macro>
      <let><var n="caseSecondWord"/><var n="chunkCase"/></let>
    </def-macro>
    <def-macro n="set_caseSecondWordCmp3" npar="3">
      <let><var n="chunkCase"/><lit v="aa"/></let> <!-- ie. we're not the first word of sentence -->
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="3"/></call-macro>
      <let><var n="caseSecondWord"/><var n="chunkCase"/></let>
       <choose><when>
        <test><equal><var n="upperMiddleParts"/><lit v="yes"/></equal></test>
        <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
      </when></choose>
    </def-macro>
    <def-macro n="set_caseSecondWordCmp4" npar="4">
      <let><var n="chunkCase"/><lit v="aa"/></let> <!-- ie. we're not the first word of sentence -->
      <call-macro n="set_chunkCaseCmp"><with-param pos="1"/><with-param pos="4"/></call-macro>
      <let><var n="caseSecondWord"/><var n="chunkCase"/></let>
      <choose><when>
        <test><equal><var n="upperMiddleParts"/><lit v="yes"/></equal></test>
        <modify-case><clip pos="2" side="tl" part="lem"/><lit v="AA"/></modify-case>
        <modify-case><clip pos="3" side="tl" part="lem"/><lit v="AA"/></modify-case>
      </when></choose>
    </def-macro>

    <def-macro n="set_caseFirstWord2" npar="2"
               c="arg1: first word, arg2: second word">
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="2"/></call-macro>
    </def-macro>
    <def-macro n="set_caseFirstWord3" npar="3"
               c="arg1: first word, arg2: second word, arg3: third word">
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="3"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="2"/></call-macro>
    </def-macro>
    <def-macro n="set_caseFirstWord4" npar="4"
               c="arg1: first word, arg2: second word, arg3: third word, arg4: fourth word">
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="4"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="3"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="2"/></call-macro>
    </def-macro>
    <def-macro n="set_caseFirstWord5" npar="5"
               c="arg1: first word, arg2: second word, arg3: third word, arg4: fourth word, etc.">
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="5"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="4"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="3"/></call-macro>
      <call-macro n="set_caseSecondWord"><with-param pos="2"/></call-macro>
    </def-macro>

    <def-macro n="set_lemq" npar="1" c="make lemq its own chunk iff we're translating from non-mwe to mwe">
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="sl" part="lemq"/><lit v=""/></equal>
              <not><equal><clip pos="1" side="tl" part="lemq"/><lit v=""/></equal></not>
            </and>
          </test>
          <let><var n="c_lemq"/><lit v=""/></let>
          <choose>
            <when>
              <test><and>
                <in><clip pos="1" side="tl" part="lem"/><list n="skip-n-lemqverbs"/></in>
                <in><clip pos="1" side="tl" part="temps"/><list n="pres-pret"/></in>
              </and></test>
              <let><var n="lemq-skip"/><lit-tag v="lemq-skip-n"/></let>
            </when>
            <otherwise><let><var n="lemq-skip"/><lit v=""/></let></otherwise>
          </choose>
          <choose><when>
            <test><in><clip pos="1" side="tl" part="lem"/><list n="skip-obj-lemqverbs"/></in></test>
            <append n="lemq-skip"><lit-tag v="lemq-skip-obj"/></append>
          </when></choose>
          <let>
            <var n="out_lemq"/>
            <concat>
              <lit v="^lemq"/>
              <lit-tag v="vblex.lemq"/>
              <var n="lemq-skip"/>
              <lit v="{^"/>
              <clip pos="1" side="tl" part="lemq"/>
              <lit v="$}$"/>
            </concat>
          </let>
        </when>
        <otherwise>
          <let><var n="c_lemq"/><clip pos="1" side="tl" part="lemq"/></let>
          <let><var n="out_lemq"/><lit v=""/></let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="set_aux" npar="1"
               c="arg.1: main/content verb">
      <let><var n="aux"/><lit v=""/></let>
      <choose>
        <when>
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="vere_pass"/></in></test>
          <let><var n="aux"/><lit v="^vere"/></let>
        </when>
        <otherwise>
          <let><var n="aux"/><lit v="^bli"/></let>
        </otherwise>
      </choose>
      <append n="aux">
        <clip pos="1" side="tl" part="a_verb"/>
        <clip pos="1" side="tl" part="temps"/>
        <lit v="$"/>
      </append>
    </def-macro>
    <def-macro n="set_aux2" npar="2"
               c="arg.1: main/content verb; arg.2: gives case">
      <let><var n="aux"/><lit v=""/></let>
      <choose>
        <when>
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="vere_pass"/></in></test>
          <let><var n="aux"/><get-case-from pos="2"><lit v="^vere"/></get-case-from></let>
        </when>
        <otherwise>
          <let><var n="aux"/><get-case-from pos="2"><lit v="^bli"/></get-case-from></let>
        </otherwise>
      </choose>
      <append n="aux">
        <clip pos="1" side="tl" part="a_verb"/>
        <clip pos="1" side="tl" part="temps"/>
        <lit v="$"/>
      </append>
    </def-macro>

    <!-- Macros which set concordance variables: -->
    <def-macro n="set_gender1.adj" npar="1"
               c="arg.1: adj">
      <choose><when>
        <test><or>
          <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="m"/></equal>
          <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="GD"/></equal>
        </or></test>
        <let><clip pos="1" side="tl" part="gen"/><lit-tag v="mf"/></let>
      </when></choose>
    </def-macro>

    <def-macro n="set_gender2.n.adj" npar="2"
               c="arg.1: noun, arg.2: adjective
                  Only take noun gender if adj is GD (in which case we may have adj.f in nn.dix),
                  or if noun has changed gender (if not GD, don't allow adj.f as target).
                  (We want to allow 'helt.nt svart.mf katt.m', no gender change nor GD on 'helt',
                   but o/w make sure adj corresponds: 'helt.nt forfatterskap => heil.mf forfattarskap')">
      <choose><when>
        <test><and>
          <in><clip pos="1" side="tl" part="gen"/><list n="adj-genders"/></in>
          <or>
            <in><clip pos="2" side="tl" part="lem"/><list n="adj-f"/></in>
            <not><equal><clip pos="1" side="sl" part="gen"/><clip pos="1" side="tl" part="gen"/></equal></not>
          </or>
        </and></test>
        <choose>
          <when>
            <test><and>
              <not><in><clip pos="2" side="tl" part="lem"/><list n="adj-f"/></in></not>
              <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="f"/></equal>
            </and></test>
            <let><clip pos="2" side="tl" part="gen"/><lit-tag v="mf"/></let>
          </when>
          <otherwise>
            <let><clip pos="2" side="tl" part="gen"/><clip pos="1" side="tl" part="gen"/></let>
          </otherwise>
        </choose>
      </when>
      </choose>
      <call-macro n="set_gender1.adj"><with-param pos="2"/></call-macro>
    </def-macro>

    <def-macro n="set_gender2.n.pre-adj" npar="2"
               c="arg.1: noun, arg.2: adjective before adjective
                  For use on adjectives that appear before other adjectives – very often adverbial usage.
                  Do set_gender2.n.adj, but only allow changing m/f→nt (with certain lemma-based exceptions)
                  (if nob adj is nt, assume adverbial and keep nt even if noun changed gender to m/f, e.g. «helt fint sted» → «heilt(not heil!) fin stad»)">
      <choose>
        <when>
          <test><or>
            <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="nt"/></equal>
            <and>
              <in><clip pos="2" side="tl" part="lem"/><list n="adj-f"/></in>
              <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="f"/></equal>
            </and>
            <in><clip pos="2" side="tl" part="lemh"/><list n="non-adverbial-adjs"/></in>
          </or></test>
          <call-macro n="set_gender2.n.adj"><with-param pos="1"/><with-param pos="2"/></call-macro>
        </when>
        <otherwise>
          <call-macro n="set_gender1.adj"><with-param pos="2"/></call-macro>
        </otherwise>
      </choose>
    </def-macro>



    <def-macro n="set_grau_aux1" npar="1"
               c="arg.1: adjective
                  When translating from sint to non-sint, put «meir/mest» in grau_aux-variable.

                  Assumes c_name has been set!">
      <choose>
        <when>
          <test><and>
            <equal><clip pos="1" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
            <not><equal><clip pos="1" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
            <or>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="comp"/></equal>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal>
            </or>
          </and></test>

          <let>
            <var n="grau_aux"/>
            <concat>
              <lit v="^mykje"/>
              <lit-tag v="adj"/>
              <lit-tag v="sint"/>
              <clip pos="1" side="tl" part="grau"/>
              <lit-tag v="un"/>
              <lit-tag v="sp"/>
            </concat>
          </let>
          <choose><when>
            <test><equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal></test>
            <append n="grau_aux"><lit-tag v="ind"/></append>
          </when></choose>
          <append n="grau_aux"><lit v="$ "/></append>

          <let>
            <var n="c_name"/>
            <concat>
              <lit v="adj_"/>
              <var n="c_name"/>
            </concat>
          </let>

          <choose>
            <when>
              <test><equal><clip pos="1" side="sl" part="art"/><lit-tag v="def"/></equal></test>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="adj.pst.un.sp.def"/></let>
            </when>
            <otherwise>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="adj.pst.nt.sg.ind"/></let>
            </otherwise>
          </choose>
        </when>

        <otherwise>
          <let>
            <var n="grau_aux"/><lit v=""/>
          </let>
        </otherwise>

      </choose>
    </def-macro>

    <def-macro n="set_grau_aux2" npar="2"
               c="arg.1: adjective arg.2: adjective
                  When translating from «meir/mest» non-sint to sint, remove «meir/mest», empty grau_aux-variable.
                  When translating from (first arg) sint to non-sint, put «meir/mest» in grau_aux-variable.

                  Assumes c_name has been set!">
      <choose>
        <when>
          <test><and>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="mykje"/></equal>
            <or>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="comp"/></equal>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal>
            </or>
            <not><equal><clip pos="2" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
            <equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
          </and></test>
          <!-- The second adj is synthetic, so copy tags from first adj: -->
          <let><var n="defnes"/><clip pos="2" side="tl" part="art"/></let>
          <let><clip pos="2" side="tl" part="tags"/><clip pos="1" side="tl" part="tags"/></let>
          <let><clip pos="2" side="tl" part="art"/><var n="defnes"/></let>
          <!-- And delete the first (meir/mest) adj: -->
          <let><clip pos="1" side="tl" part="whole"/><lit v=""/></let>
          <let><var n="grau_aux"/><lit v=""/></let>
        </when>

        <otherwise>
          <!-- Or maybe we want to _add_ meir/mest: -->
          <call-macro n="set_grau_aux1"><with-param pos="1"/></call-macro>
          <!-- and fix tags on the second adj (but no new aux if only second adj is non-pst; TODO better) -->
          <choose><when>
            <test><and>
              <equal><clip pos="2" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
              <not><equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
              <or>
                <equal><clip pos="2" side="tl" part="grau"/><lit-tag v="comp"/></equal>
                <equal><clip pos="2" side="tl" part="grau"/><lit-tag v="sup"/></equal>
              </or>
            </and></test>
            <choose>
              <when>
                <test><equal><clip pos="2" side="sl" part="art"/><lit-tag v="def"/></equal></test>
                <let><clip pos="2" side="tl" part="tags"/><lit-tag v="adj.pst.un.sp.def"/></let>
              </when>
              <otherwise>
                <let><clip pos="2" side="tl" part="tags"/><lit-tag v="adj.pst.nt.sg.ind"/></let>
              </otherwise>
            </choose>
          </when></choose>
        </otherwise>
      </choose>
    </def-macro>

    <!-- Output macros: -->
    <def-macro n="out_ndef" npar="2">
      <!-- arg.1: possessed noun. arg.2 may change typographic case

           Strip case, add definiteness.  -->
      <choose>
        <when>
          <test><not>
            <and>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="De"/></equal>
              <equal><clip pos="2" side="tl" part="a_det"/><lit-tag v="det.pos"/></equal>
              <equal><clip pos="2" side="tl" part="persona"/><lit-tag v="p2"/></equal>
            </and>
          </not></test>
        </when>
      </choose>
      <let><clip pos="1" side="tl" part="art"/><lit-tag v="def"/></let>
      <out>
        <chunk name="nn" case="caseFirstWord">
          <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_nom"/>
            <clip pos="1" side="tl" part="gen"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="art"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </def-macro>

    <def-macro n="out_posgen_subj" npar="2">
      <!-- arg.1: possessor, either genitive noun or det.pos.
           arg.2: gives typographic case
           Assumes no variables.
           Turns eg. "min" into "eg", "naboens" into "naboen".
           Strips case both from chunk and word.
      -->
      <choose>
        <when>
          <test><equal><clip pos="1" side="sl" part="a_det"/><lit-tag v="det.pos"/></equal></test>
          <call-macro n="set_caseFirstWord"><with-param pos="2"/></call-macro>
          <choose>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="min"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="eg"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p1.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="din"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="du"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p2.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="sin"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="seg"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.ref.p3.un.sp.acc"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="hennar"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="ho"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.f.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="hans"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="han"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.m.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dens"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="den"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dets"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="den"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.nt.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="vår"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="vi"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p1.un.pl.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dykkar"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="de"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p2.un.pl.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="deira"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="dei"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.un.pl.nom"/></let>
