    <!-- ADJ CM ADJ N -->
    <rule comment="ADJ CM ADJ N">
      <pattern>
        <pattern-item n="adj"/>  <!-- 1 kalde -->
        <pattern-item n="cm"/>   <!-- 2 , -->
        <pattern-item n="adj"/>  <!-- 3 fine -->
        <pattern-item n="n_np"/> <!-- 4 tida-->
      </pattern>
      <action>
        <choose><when c="Don't chunk «generelt lite fugl» etc.">
          <test><or>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v=""/></equal>
            <equal><clip pos="3" side="tl" part="lemh"/><lit v="mange"/></equal> <!-- [stukket] [flere ganger] -->
            <equal><clip pos="3" side="tl" part="lemh"/><lit v="mye"/></equal>   <!-- [tilbudt] [mye penger] -->
            <and>
              <not><in><clip pos="1" side="tl" part="lemh"/><list n="adj-ind-def"/></in></not>
              <not><equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal></not> <!-- allow «tettest/mest befolkede områdene» -->
              <equal><clip pos="1" side="tl" part="art"/><lit-tag v="ind"/></equal>
              <equal><clip pos="4" side="tl" part="art"/><lit-tag v="def"/></equal>
            </and>
            <and>
              <not><in><clip pos="1" side="tl" part="lemh"/><list n="adj-ind-def"/></in></not>
              <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="nt"/></equal>
              <equal><clip pos="3" side="sl" part="gen"/><lit-tag v="nt"/></equal>
              <in><clip pos="4" side="sl" part="gen"/><list n="noun-gender-not-adjnt"/></in>
            </and>
          </or></test>
          <reject-current-rule shifting="no"/>
        </when></choose>
        <call-macro n="set_gender2.n.adj"><with-param pos="4"/><with-param pos="1"/></call-macro> <!-- was set_gender2.n.pre-adj, but before comma the adj should be unambiguously modifying noun -->
        <call-macro n="set_gender2.n.adj"><with-param pos="4"/><with-param pos="3"/></call-macro>

        <!-- Start building c_name from the rightmost adjective (set_grau_aux1 may prepend to c_name): -->
        <let><var n="c_name"/><lit v="adj_n"/></let>
