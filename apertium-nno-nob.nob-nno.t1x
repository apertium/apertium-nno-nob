  <section-def-macros>

    <def-macro n="flush-mwe-queue" npar="1">
      <choose><when>
        <test><not><equal><var n="mwe-queue"/><lit v=""/></equal></not></test>
        <choose><when c="output space before, when before punctuation">
          <test><not><equal><clip pos="1" side="tl" part="a_sent"/><lit v=""/></equal></not></test>
          <out><b/></out>
        </when></choose>
        <out><var n="mwe-queue"/></out>
        <choose><when c="output space after, unless before punctuation">
          <test><equal><clip pos="1" side="tl" part="a_sent"/><lit v=""/></equal></test>
          <out><b/></out>
        </when></choose>
        <let><var n="mwe-queue"/><lit v=""/></let>
      </when></choose>
    </def-macro>

    <def-macro n="bigram_genprep" npar="1"
               c="arg.1: possibly owned noun. Helper for stash_case_in_ntags">
      <!-- TODO: More general handling of owned keepgenx: -->
      <choose><when>
        <test><in caseless="yes"><clip pos="1" side="tl" part="lemh"/><list n="keepgenx"/></in></test>
        <append n="ntags"><lit-tag v="keepgenx"/></append>
      </when></choose>
      <!-- Override gen-prep on prevlem by looking up bigram -->
      <let><var n="bilem"/><concat><clip pos="1" side="tl" part="lem"/><lit v="_"/><var n="prevlem"/></concat></let>
      <modify-case><var n="bilem"/><lit v="aa"/></modify-case>
      <choose>
        <when><test><in><var n="bilem"/><list n="bi-gen-til"/></in></test><append n="ntags"><lit-tag v="pr_til"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-av2"/></in></test><append n="ntags"><lit-tag v="pr_av"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-frå2"/></in></test><append n="ntags"><lit-tag v="pr_frå"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-på2"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
        <when c="Some prepositions only on plurals – «blant menn» but not *«blant mannen»">
          <test><and>
            <equal><var n="prevnbr"/><lit-tag v="pl"/></equal>
            <in><var n="bilem"/><list n="bi-gen-blant"/></in>
          </and></test>
          <append n="ntags"><lit-tag v="pr_blant"/></append>
        </when>
        <when c="Some prepositions only on plurals – «mellom menn» but not *«mellom mannen»">
          <test><and>
            <equal><var n="prevnbr"/><lit-tag v="pl"/></equal>
            <in><var n="bilem"/><list n="bi-gen-mellom"/></in>
          </and></test>
          <append n="ntags"><lit-tag v="pr_mellom"/></append>
        </when>
        <!-- dev/genitive-prepositions: -->
        <when><test><in><var n="bilem"/><list n="bi-gen-av"/></in></test><append n="ntags"><lit-tag v="pr_av"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-etter"/></in></test><append n="ntags"><lit-tag v="pr_etter"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-for"/></in></test><append n="ntags"><lit-tag v="pr_for"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-frå"/></in></test><append n="ntags"><lit-tag v="pr_frå"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-før"/></in></test><append n="ntags"><lit-tag v="pr_før"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-gjennom"/></in></test><append n="ntags"><lit-tag v="pr_gjennom"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-hos"/></in></test><append n="ntags"><lit-tag v="pr_hos"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-i"/></in></test><append n="ntags"><lit-tag v="pr_i"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-med"/></in></test><append n="ntags"><lit-tag v="pr_med"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-mot"/></in></test><append n="ntags"><lit-tag v="pr_mot"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-om"/></in></test><append n="ntags"><lit-tag v="pr_om"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-over"/></in></test><append n="ntags"><lit-tag v="pr_over"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-per"/></in></test><append n="ntags"><lit-tag v="pr_per"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-på"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-rundt"/></in></test><append n="ntags"><lit-tag v="pr_rundt"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-som"/></in></test><append n="ntags"><lit-tag v="pr_som"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-uansett"/></in></test><append n="ntags"><lit-tag v="pr_uansett"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-under"/></in></test><append n="ntags"><lit-tag v="pr_under"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-unna"/></in></test><append n="ntags"><lit-tag v="pr_unna"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-ut"/></in></test><append n="ntags"><lit-tag v="pr_ut"/></append></when>
        <when><test><in><var n="bilem"/><list n="bi-gen-ved"/></in></test><append n="ntags"><lit-tag v="pr_ved"/></append></when>
        <otherwise>
          <let><var n="lemma"/><clip pos="1" side="tl" part="lem"/></let>
          <modify-case><var n="lemma"/><lit v="aa"/></modify-case>
          <choose><when c="Only use obj unigram if both out_gen-prep and bi-gen didn't pick something first">
            <test><equal><var n="gen-prep"/><lit v="til"/></equal></test>
            <choose>
              <when><test><in><var n="lemma"/><list n="obj-gen-frå2"/></in></test><append n="ntags"><lit-tag v="pr_frå"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-med2"/></in></test><append n="ntags"><lit-tag v="pr_med"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-av2"/></in></test><append n="ntags"><lit-tag v="pr_av"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-på2"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-av"/></in></test><append n="ntags"><lit-tag v="pr_av"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-bak"/></in></test><append n="ntags"><lit-tag v="pr_bak"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-for"/></in></test><append n="ntags"><lit-tag v="pr_for"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-frå"/></in></test><append n="ntags"><lit-tag v="pr_frå"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-gjennom"/></in></test><append n="ntags"><lit-tag v="pr_gjennom"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-i"/></in></test><append n="ntags"><lit-tag v="pr_i"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-med"/></in></test><append n="ntags"><lit-tag v="pr_med"/></append></when>
              <when><test><in><var n="lemma"/><list n="obj-gen-på"/></in></test><append n="ntags"><lit-tag v="pr_på"/></append></when>
            </choose>
          </when></choose>
        </otherwise>
      </choose>
      <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      <let><var n="prevnbr"/><clip pos="1" side="tl" part="nbr"/></let>
    </def-macro>

    <def-macro n="stash_case_in_ntags" npar="1"
               c="arg.1: noun. Puts tags of noun in ntags (for use as chunk tags).">
      <let><var n="ntags"/><clip pos="1" side="tl" part="tags"/></let>
      <choose><when>
        <test><equal><clip pos="1" side="tl" part="a_nom"/><lit-tag v="n"/></equal></test>
        <choose><when>
          <test><not><equal><clip pos="1" side="tl" part="cas"/><lit v=""/></equal></not></test>
          <append n="ntags"><lit-tag v="casedrop"/></append>
        </when></choose>
        <choose><when>
          <test><not><equal><clip pos="1" side="tl" part="a_defdrop"/><lit v=""/></equal></not></test>
          <let><clip pos="1" side="tl" part="a_defdrop"/><lit v=""/></let>
        </when></choose>
        <choose>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="nt"/></equal></test>
            <append n="ntags"><lit-tag v="sl-nt"/></append>
          </when>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="m"/></equal></test>
            <append n="ntags"><lit-tag v="sl-m"/></append>
          </when>
          <when>
            <test><equal><clip pos="1" side="sl" part="gen"/><lit-tag v="f"/></equal></test>
            <append n="ntags"><lit-tag v="sl-f"/></append>
          </when>
        </choose>
      </when></choose>
      <call-macro n="bigram_genprep"><with-param pos="1"/></call-macro>
    </def-macro>

    <def-macro n="switch_cases" npar="2">
      <choose>
        <when>
          <test><not>
            <and>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="De"/></equal>
              <equal><clip pos="2" side="tl" part="a_det"/><lit-tag v="det.pos"/></equal>
              <equal><clip pos="2" side="tl" part="persona"/><lit-tag v="p2"/></equal>
            </and>
          </not></test>
          <let><var n="caseFirstWord"/><get-case-from pos="1"><clip pos="1" side="tl" part="lemh"/></get-case-from></let>
          <let><clip pos="1" side="tl" part="lemh"/><get-case-from pos="2"><clip pos="1" side="tl" part="lemh"/></get-case-from></let>
          <modify-case><clip pos="2" side="tl" part="lemh"/><var n="caseFirstWord"/></modify-case>
        </when>
      </choose>
    </def-macro>

    <def-macro n="set_caseFirstWord" npar="1"
               c="Downcases the argument, unless np or UPPERCASE">
      <!-- TODO: Put a marker for acronyms where we don't want to copy case from them; then drop the tag -->
      <choose>
        <when c="Contains a number: don't downcase, set caseFirstWord if first in sentence else empty it">
          <test><or>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="0"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="1"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="2"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="3"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="4"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="5"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="6"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="7"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="8"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="9"/></contains-substring>
            <contains-substring><clip pos="1" side="tl" part="lemh"/><lit v="-"/></contains-substring>
            <ends-with><clip pos="1" side="tl" part="lemh"/><lit v="-basert"/></ends-with> <!-- A-Z-basert -->
            <ends-with><clip pos="1" side="tl" part="lemh"/><lit v="-ulykke"/></ends-with> <!-- A-Z-ulykka -->
          </or></test>
          <choose>
            <when>
              <test><equal><var n="caseFirstWord"/><lit v=""/></equal></test>
              <let><var n="caseFirstWord"/><get-case-from pos="1"><lit v="aa"/></get-case-from></let>
            </when>
            <otherwise>
              <let><var n="caseFirstWord"/><lit v=""/></let>
            </otherwise>
          </choose>
        </when>
        <when c="Not proper, but titlecased: downcase, set caseFirstWord Aa">
          <test><and>
            <not><begins-with><clip pos="1" side="tl" part="a_nom"/><lit-tag v="np"/></begins-with></not>
            <equal><lit v="Aa"/><get-case-from pos="1"><lit v="aa"/></get-case-from></equal>
          </and></test>
          <modify-case><clip pos="1" side="tl" part="lemh"/><lit v="aa"/></modify-case>
          <let><var n="caseFirstWord"/><lit v="Aa"/></let>
        </when>
        <when c="Proper, not first word in sentence: don't downcase, set caseFirstWord aa">
          <test><and>
            <not><equal><var n="caseFirstWord"/><lit v=""/></equal></not>
            <begins-with><clip pos="1" side="tl" part="a_nom"/><lit-tag v="np"/></begins-with>
          </and></test>
          <let><var n="caseFirstWord"/><lit v="aa"/></let>
        </when>
        <otherwise c="Else: don't downcase, set caseFirstWord to this word's case?">
          <let><var n="caseFirstWord"/><get-case-from pos="1"><lit v="aa"/></get-case-from></let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="set_caseFirstWord2" npar="2"
               c="arg1: first word, arg2: second word
                  Downcases arg1, unless np
                  Avoids applying shouting to whole chunk if only first word shouts">
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
      <let><var n="caseSecondWord"/><get-case-from pos="2"><lit v="aa"/></get-case-from></let>
      <choose><when>
        <test><and>
          <equal><var n="caseFirstWord"/><lit v="AA"/></equal>
          <not><equal><var n="caseSecondWord"/><lit v="AA"/></equal></not>
        </and></test>
        <let><var n="caseFirstWord"/><lit v="Aa"/></let>
      </when></choose>
    </def-macro>

    <def-macro n="set_lemq" npar="1" c="make lemq its own chunk iff we're translating from non-mwe to mwe">
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="sl" part="lemq"/><lit v=""/></equal>
              <not><equal><clip pos="1" side="tl" part="lemq"/><lit v=""/></equal></not>
            </and>
          </test>
          <let><var n="c_lemq"/><lit v=""/></let>
          <choose>
            <when>
              <test><and>
                <in><clip pos="1" side="tl" part="lem"/><list n="skip-n-lemqverbs"/></in>
                <in><clip pos="1" side="tl" part="temps"/><list n="pres-pret"/></in>
              </and></test>
              <let><var n="lemq-skip"/><lit-tag v="lemq-skip-n"/></let>
            </when>
            <otherwise><let><var n="lemq-skip"/><lit v=""/></let></otherwise>
          </choose>
          <let>
            <var n="out_lemq"/>
            <concat>
              <lit v="^lemq"/>
              <lit-tag v="vblex.lemq"/>
              <var n="lemq-skip"/>
              <lit v="{^"/>
              <clip pos="1" side="tl" part="lemq"/>
              <lit v="$}$"/>
            </concat>
          </let>
        </when>
        <otherwise>
          <let><var n="c_lemq"/><clip pos="1" side="tl" part="lemq"/></let>
          <let><var n="out_lemq"/><lit v=""/></let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="set_aux" npar="1"
               c="arg.1: main/content verb">
      <let><var n="aux"/><lit v=""/></let>
      <choose>
        <when>
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="vere_pass"/></in></test>
          <let><var n="aux"/><lit v="^vere"/></let>
        </when>
        <otherwise>
          <let><var n="aux"/><lit v="^bli"/></let>
        </otherwise>
      </choose>
      <append n="aux">
        <clip pos="1" side="tl" part="a_verb"/>
        <clip pos="1" side="tl" part="temps"/>
        <lit v="$"/>
      </append>
    </def-macro>
    <def-macro n="set_aux2" npar="2"
               c="arg.1: main/content verb; arg.2: gives case">
      <let><var n="aux"/><lit v=""/></let>
      <choose>
        <when>
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="vere_pass"/></in></test>
          <let><var n="aux"/><get-case-from pos="2"><lit v="^vere"/></get-case-from></let>
        </when>
        <otherwise>
          <let><var n="aux"/><get-case-from pos="2"><lit v="^bli"/></get-case-from></let>
        </otherwise>
      </choose>
      <append n="aux">
        <clip pos="1" side="tl" part="a_verb"/>
        <clip pos="1" side="tl" part="temps"/>
        <lit v="$"/>
      </append>
    </def-macro>

    <!-- Macros which set concordance variables: -->
    <def-macro n="set_gender1.adj" npar="1"
               c="arg.1: adj">
      <choose><when>
        <test><or>
          <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="m"/></equal>
          <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="GD"/></equal>
        </or></test>
        <let><clip pos="1" side="tl" part="gen"/><lit-tag v="mf"/></let>
      </when></choose>
    </def-macro>

    <def-macro n="set_gender2.n.adj" npar="2"
               c="arg.1: noun, arg.2: adjective
                  Only take noun gender if adj is GD (in which case we may have adj.f in nn.dix),
                  or if noun has changed gender (if not GD, don't allow adj.f as target).
                  (We want to allow 'helt.nt svart.mf katt.m', no gender change nor GD on 'helt',
                   but o/w make sure adj corresponds: 'helt.nt forfatterskap => heil.mf forfattarskap')">
      <choose><when>
        <test><and>
          <in><clip pos="1" side="tl" part="gen"/><list n="adj-genders"/></in>
          <or>
            <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="GD"/></equal>
            <not><equal><clip pos="1" side="sl" part="gen"/><clip pos="1" side="tl" part="gen"/></equal></not>
          </or>
        </and></test>
        <choose>
          <when>
            <test><and>
              <not><equal><clip pos="2" side="tl" part="gen"/><lit-tag v="GD"/></equal></not>
              <equal><clip pos="1" side="tl" part="gen"/><lit-tag v="f"/></equal>
            </and></test>
            <let><clip pos="2" side="tl" part="gen"/><lit-tag v="mf"/></let>
          </when>
          <otherwise>
            <let><clip pos="2" side="tl" part="gen"/><clip pos="1" side="tl" part="gen"/></let>
          </otherwise>
        </choose>
      </when>
      </choose>
      <call-macro n="set_gender1.adj"><with-param pos="2"/></call-macro>
    </def-macro>



    <def-macro n="set_grau_aux1" npar="1"
               c="arg.1: adjective
                  When translating from sint to non-sint, put «meir/mest» in grau_aux-variable.

                  Assumes c_name has been set!">
      <choose>
        <when>
          <test><and>
            <equal><clip pos="1" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
            <not><equal><clip pos="1" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
            <or>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="comp"/></equal>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal>
            </or>
          </and></test>

          <let>
            <var n="grau_aux"/>
            <concat>
              <lit v="^mykje"/>
              <lit-tag v="adj"/>
              <lit-tag v="sint"/>
              <clip pos="1" side="tl" part="grau"/>
              <lit-tag v="un"/>
              <lit-tag v="sp"/>
            </concat>
          </let>
          <choose><when>
            <test><equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal></test>
            <append n="grau_aux"><lit-tag v="ind"/></append>
          </when></choose>
          <append n="grau_aux"><lit v="$ "/></append>

          <let>
            <var n="c_name"/>
            <concat>
              <lit v="adj_"/>
              <var n="c_name"/>
            </concat>
          </let>

          <choose>
            <when>
              <test><equal><clip pos="1" side="sl" part="art"/><lit-tag v="def"/></equal></test>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="adj.pst.un.sp.def"/></let>
            </when>
            <otherwise>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="adj.pst.nt.sg.ind"/></let>
            </otherwise>
          </choose>
        </when>

        <otherwise>
          <let>
            <var n="grau_aux"/><lit v=""/>
          </let>
        </otherwise>

      </choose>
    </def-macro>

    <def-macro n="set_grau_aux2" npar="2"
               c="arg.1: adjective arg.2: adjective
                  When translating from «meir/mest» non-sint to sint, remove «meir/mest», empty grau_aux-variable.
                  When translating from (first arg) sint to non-sint, put «meir/mest» in grau_aux-variable.

                  Assumes c_name has been set!">
      <choose>
        <when>
          <test><and>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="mykje"/></equal>
            <or>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="comp"/></equal>
              <equal><clip pos="1" side="tl" part="grau"/><lit-tag v="sup"/></equal>
            </or>
            <not><equal><clip pos="2" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
            <equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
          </and></test>
          <!-- The second adj is synthetic, so copy tags from first adj: -->
          <let><var n="defnes"/><clip pos="2" side="tl" part="art"/></let>
          <let><clip pos="2" side="tl" part="tags"/><clip pos="1" side="tl" part="tags"/></let>
          <let><clip pos="2" side="tl" part="art"/><var n="defnes"/></let>
          <!-- And delete the first (meir/mest) adj: -->
          <let><clip pos="1" side="tl" part="whole"/><lit v=""/></let>
          <let><var n="grau_aux"/><lit v=""/></let>
        </when>

        <otherwise>
          <!-- Or maybe we want to _add_ meir/mest: -->
          <call-macro n="set_grau_aux1"><with-param pos="1"/></call-macro>
          <!-- and fix tags on the second adj (but no new aux if only second adj is non-pst; TODO better) -->
          <choose><when>
            <test><and>
              <equal><clip pos="2" side="sl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
              <not><equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal></not>
              <or>
                <equal><clip pos="2" side="tl" part="grau"/><lit-tag v="comp"/></equal>
                <equal><clip pos="2" side="tl" part="grau"/><lit-tag v="sup"/></equal>
              </or>
            </and></test>
            <choose>
              <when>
                <test><equal><clip pos="2" side="sl" part="art"/><lit-tag v="def"/></equal></test>
                <let><clip pos="2" side="tl" part="tags"/><lit-tag v="adj.pst.un.sp.def"/></let>
              </when>
              <otherwise>
                <let><clip pos="2" side="tl" part="tags"/><lit-tag v="adj.pst.nt.sg.ind"/></let>
              </otherwise>
            </choose>
          </when></choose>
        </otherwise>
      </choose>
    </def-macro>

    <!-- Output macros: -->
    <def-macro n="out_ndef" npar="2">
      <!-- arg.1: possessed noun. arg.2 may change typographic case

           Strip case, add definiteness.  -->
      <choose>
        <when>
          <test><not>
            <and>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="De"/></equal>
              <equal><clip pos="2" side="tl" part="a_det"/><lit-tag v="det.pos"/></equal>
              <equal><clip pos="2" side="tl" part="persona"/><lit-tag v="p2"/></equal>
            </and>
          </not></test>
          <let><var n="caseFirstWord"/><get-case-from pos="2"><lit v="aa"/></get-case-from></let>
          <modify-case><clip pos="1" side="tl" part="lemh"/><lit v="aa"/></modify-case>
        </when>
      </choose>
      <let><clip pos="1" side="tl" part="art"/><lit-tag v="def"/></let>
      <out>
        <chunk name="n" case="caseFirstWord">
          <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_nom"/>
            <clip pos="1" side="tl" part="gen"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="art"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </def-macro>

    <def-macro n="out_posgen_subj" npar="2">
      <!-- arg.1: possessor, either genitive noun or det.pos.
           arg.2: gives typographic case
           Assumes no variables.
           Turns eg. "min" into "eg", "naboens" into "naboen".
           Strips case both from chunk and word.
      -->
      <choose>
        <when>
          <test><equal><clip pos="1" side="sl" part="a_det"/><lit-tag v="det.pos"/></equal></test>
          <call-macro n="set_caseFirstWord"><with-param pos="2"/></call-macro>
          <choose>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="min"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="eg"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p1.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="din"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="du"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p2.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="sin"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="seg"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.ref.p3.un.sp.acc"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="hennar"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="ho"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.f.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="hans"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="han"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.m.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dens"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="den"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.mf.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dets"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="den"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.nt.sg.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="vår"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="vi"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p1.un.pl.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="dykkar"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="de"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p2.un.pl.nom"/></let>
            </when>
            <when>
              <test><equal><clip pos="1" side="tl" part="lemh"/><lit v="deira"/></equal></test>
              <let><clip pos="1" side="tl" part="lemh"/><lit v="dei"/></let>
              <let><clip pos="1" side="tl" part="tags"/><lit-tag v="prn.pers.p3.un.pl.nom"/></let>
            </when>
          </choose>
          <out>
            <chunk name="prn" case="caseFirstWord">
              <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
              <lu>
                <clip pos="1" side="tl" part="lemh"/>
                <clip pos="1" side="tl" part="tags"/>
              </lu>
            </chunk>
          </out>
        </when>
        <otherwise>
          <call-macro n="set_caseFirstWord"><with-param pos="2"/></call-macro>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
          <out>
            <chunk name="n" case="caseFirstWord">
              <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
              <lu>
                <get-case-from pos="2"><clip pos="1" side="tl" part="lemh"/></get-case-from>
                <clip pos="1" side="tl" part="tags"/>
                <clip pos="1" side="tl" part="lemq"/>
              </lu>
            </chunk>
          </out>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="out_gen-prep" npar="1"
               c="arg.1: noun. Only acts on genitives. Adds a space after.">
      <let><var n="lemma"/><clip pos="1" side="tl" part="lemh"/></let>
      <modify-case><var n="lemma"/><lit v="aa"/></modify-case>
      <choose><when c="gen">
        <test><and>
          <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="gen"/></equal>
          <equal><clip pos="1" side="tl" part="a_nom"/><lit-tag v="n"/></equal>
        </and></test>
        <choose>
          <!-- Manual lists first: -->
          <when c="i+defdrop">
            <test><in caseless="yes"><var n="lemma"/><list n="ind-gen-i"/></in></test>
            <let><clip pos="1" side="tl" part="tags"/><concat><clip pos="1" side="tl" part="tags"/><lit-tag v="defdrop"/></concat></let>
            <let><var n="gen-prep"/><lit v="i"/></let>
          </when>
          <when c="på+defdrop">
            <test><in><var n="lemma"/><list n="ind-gen-på"/></in></test>
            <let><clip pos="1" side="tl" part="tags"/><concat><clip pos="1" side="tl" part="tags"/><lit-tag v="defdrop"/></concat></let>
            <let><var n="gen-prep"/><lit v="på"/></let>
          </when>
          <when><test><in><var n="lemma"/><list n="gen-i2"/></in></test><let><var n="gen-prep"/><lit v="i"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-på2"/></in></test><let><var n="gen-prep"/><lit v="på"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-av2"/></in></test><let><var n="gen-prep"/><lit v="av"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-for2"/></in></test><let><var n="gen-prep"/><lit v="for"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-frå2"/></in></test><let><var n="gen-prep"/><lit v="frå"/></let></when>
          <when c="Some prepositions only on plurals – «blant menn» but not *«blant mannen»">
            <test><and>
              <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
              <in><var n="lemma"/><list n="gen-blant"/></in>
            </and></test>
            <let><var n="gen-prep"/><lit v="blant"/></let>
          </when>
          <when c="Some prepositions only on plurals – «mellom menn» but not *«mellom mannen»">
            <test><and>
              <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
              <in><var n="lemma"/><list n="gen-mellom"/></in>
            </and></test>
            <let><var n="gen-prep"/><lit v="mellom"/></let>
          </when>
          <!-- dev/genitive-prepositions: -->
          <when><test><in><var n="lemma"/><list n="gen-av"/></in></test><let><var n="gen-prep"/><lit v="av"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-bak"/></in></test><let><var n="gen-prep"/><lit v="bak"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-etter"/></in></test><let><var n="gen-prep"/><lit v="etter"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-for"/></in></test><let><var n="gen-prep"/><lit v="for"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-frå"/></in></test><let><var n="gen-prep"/><lit v="frå"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-gjennom"/></in></test><let><var n="gen-prep"/><lit v="gjennom"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-hos"/></in></test><let><var n="gen-prep"/><lit v="hos"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-i"/></in></test><let><var n="gen-prep"/><lit v="i"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-innan"/></in></test><let><var n="gen-prep"/><lit v="innan"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-om"/></in></test><let><var n="gen-prep"/><lit v="om"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-på"/></in></test><let><var n="gen-prep"/><lit v="på"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-som"/></in></test><let><var n="gen-prep"/><lit v="som"/></let></when>
          <when><test><in><var n="lemma"/><list n="gen-ved"/></in></test><let><var n="gen-prep"/><lit v="ved"/></let></when>

          <otherwise c="til">
            <let><var n="gen-prep"/><lit v="til"/></let>
          </otherwise>
        </choose>
        <out>
          <chunk name="gen-prep">
            <tags><tag><lit-tag v="pr"/></tag></tags>
            <lu>
              <var n="gen-prep"/>
              <lit-tag v="pr"/>
            </lu>
          </chunk>
          <b/>
        </out>
      </when></choose>
    </def-macro>


    <def-macro n="gen2cmp" npar="2"
               c="arg.1: genitive noun
                  arg.2: owned noun">
      <choose><when c="avoid samfunnsverdet, regjeringsverdet, kyrkjeverdet">
        <test><and>
          <equal><clip pos="2" side="tl" part="lemh"/><lit v="verd"/></equal>
          <not><or>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="menneske"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="individ"/></equal>
          </or></not>
        </and></test>
        <reject-current-rule shifting="no"/>
      </when></choose>

      <let><var n="c_name"/><lit v="n_n"/></let>
      <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>

      <call-macro n="stash_case_in_ntags"><with-param pos="1"/></call-macro>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="sp"/></equal></test>
          <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sp.cmp"/></let>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
        </when>
        <otherwise>
          <let><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></let>
          <let><clip pos="1" side="tl" part="art"/><lit-tag v="ind.cmp"/></let>
          <let><clip pos="1" side="tl" part="cas"/><lit v=""/></let>
        </otherwise>
      </choose>
      <let><clip pos="2" side="tl" part="art"/><lit-tag v="def"/></let>
      <out>
        <chunk namefrom="c_name">
          <tags>
            <tag><clip pos="2" side="tl" part="tags"/></tag>
          </tags>
          <lu><clip pos="1" side="tl" part="whole"/></lu>
          <lu><clip pos="2" side="tl" part="whole"/></lu>
        </chunk>
      </out>
      <choose><when>
        <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
        <out><b pos="1"/></out>
      </when></choose>
    </def-macro>

  </section-def-macros>



  <section-rules>
    <rule comment="PÅ MANDAG: på måndag => måndag
                   Unntak: særlig på søndag, etter hvert på onsdag, – på lørdag
                   Tag as adv for interchunk rules (TODO: actual syntactical tagging)">
      <pattern>
        <pattern-item n="på"/>
        <pattern-item n="nind_vekedag"/>
      </pattern>
      <action>
        <choose><when c="Don't drop for 'etter hvert på søndag'">
          <test><or>
            <equal caseless="yes"><var n="prevlem"/><lit v="etter kvart"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="etterkvart"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="spesielt"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="spesiell"/></equal>
            <equal caseless="yes"><var n="prevlem"/><lit v="–"/></equal>
          </or></test>
          <reject-current-rule shifting="no"/>
        </when></choose>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <choose><when>
          <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
          <out><b pos="1"/></out>
        </when></choose>
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="adv"/></tag>
              <tag><clip pos="2" side="tl" part="tags"/></tag>
            </tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="2" side="tl" part="lem"/></let>
      </action>
    </rule>
    <rule comment="MANDAG: måndag => måndag
                   Tag as adv for interchunk rules (TODO: actual syntactical tagging)">
      <pattern>
        <pattern-item n="nind_vekedag"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="adv"/></tag>
              <tag><clip pos="1" side="tl" part="tags"/></tag>
            </tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      </action>
    </rule>

    <!-- Idiomatic rules first -->
    <rule comment="ALLE OG ENHVER AV PRN → PRN ALLE">
      <pattern>
        <pattern-item n="alle_og_enhver"/>
        <pattern-item n="av"/>
        <pattern-item n="prn"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="prn" case="caseFirstWord">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="det" case="caseSecondWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>
    
    <rule comment="DENNE NGEN_PERIODE: denne ukens [kongress] → [kongressen] denne veka">
      <pattern>
        <pattern-item n="denne"/>
        <pattern-item n="ngen_periode"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="stash_case_in_ntags"><with-param pos="2"/></call-macro>
        <out>
          <chunk name="gen-prep" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="lemh"/><clip pos="1" side="tl" part="tags"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="n" case="caseSecondWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="hvilken som helst by → kva by som helst
                   put lemq after">
      <pattern>
        <pattern-item n="hvilken_som_helst"/>
        <pattern-item n="n_np"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="out_gen-prep"><with-param pos="2"/></call-macro>
        <call-macro n="stash_case_in_ntags"><with-param pos="2"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="adj" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="lemh"/><clip pos="1" side="tl" part="tags"/><var n="c_lemq"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="n" case="caseSecondWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <var n="out_lemq"/>
        </out>
      </action>
    </rule>
    <rule comment="en hvilken som helst by → kva by som helst
                   put lemq after
                   and drop DETQNT (should we instead tag the adj as det-drop and do it in t2x?)">
      <pattern>
        <pattern-item n="detqnt"/>
        <pattern-item n="hvilken_som_helst"/>
        <pattern-item n="n_np"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="out_gen-prep"><with-param pos="3"/></call-macro>
        <call-macro n="stash_case_in_ntags"><with-param pos="3"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="2"/></call-macro>
        <out>
          <chunk name="adj" case="caseFirstWord">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="lemh"/><clip pos="2" side="tl" part="tags"/><var n="c_lemq"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="n" case="caseSecondWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
          <var n="out_lemq"/>
        </out>
        <choose><when><test><not><equal><b pos="2"/><lit v=" "/></equal></not></test><out><b pos="3"/></out></when></choose>
      </action>
    </rule>

    <rule comment="en hvilken som helst annen → kva som helst annan
                   drop DETQNT (should we instead tag the adj as det-drop and do it in t2x?)">
      <pattern>
        <pattern-item n="detqnt"/>
        <pattern-item n="hvilken_som_helst_annen"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose><when>
          <test><equal><clip pos="2" side="tl" part="lemh"/><lit v="alle andre"/></equal></test>
          <let><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></let>
          <let><clip pos="2" side="tl" part="gen"/><lit-tag v="un"/></let>
        </when></choose>
        <out>
          <chunk name="det2adj" case="caseFirstWord">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <choose><when><test><not><equal><b pos="1"/><lit v=" "/></equal></not></test><out><b pos="1"/></out></when></choose>
      </action>
    </rule>

    <rule comment="anført av X → med X i spissen
                   Sets variable `mwe-queue`, which has to be output before the next finite verb or comma or period.">
      <pattern>
        <pattern-item n="anført"/>
        <pattern-item n="av"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="pr" case="caseFirstWord">
            <tags><tag><lit-tag v="pr.med"/></tag></tags>
            <lu>
              <lit v="med"/>
              <lit-tag v="pr"/>
            </lu>
          </chunk>
        </out>
        <choose><when>
          <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
          <out><b pos="1"/></out>
        </when></choose>
        <let>
          <var n="mwe-queue"/>
          <chunk name="default">
            <tags><tag><lit-tag v="default"/></tag></tags>
            <lu>
              <lit v="i"/>
              <lit-tag v="pr"/>
            </lu>
            <b/>
            <lu>
              <lit v="spiss"/>
              <lit-tag v="n.m.sg.def"/>
            </lu>
          </chunk>
        </let>
      </action>
    </rule>

    <rule comment="har angivelig → skal ha">
      <pattern>
        <pattern-item n="har"/>
        <pattern-item n="angivelig"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="aux" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu>
              <lit v="skulle"/>
              <clip pos="1" side="tl" part="tags"/>
            </lu>
          </chunk>
          <b pos="1"/>
          <chunk name="aux" case="caseSecondWord">
            <tags><tag><lit-tag v="vblex.inf"/></tag></tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <lit-tag v="vblex.inf"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="et antall → fleire
                   idiomatic rule">
      <pattern>
        <pattern-item n="et"/>
        <pattern-item n="antall"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose>
          <when c="fleire">
            <test><equal><clip pos="2" side="tl" part="lemh"/><lit v="fleire"/></equal></test>
            <out>
              <chunk name="adj" case="caseFirstWord">
                <tags><tag><lit-tag v="adj.sint.comp.un.sp"/></tag></tags>
                <lu>
                  <lit v="mange"/>
                  <lit-tag v="adj.sint.comp.un.sp"/>
                </lu>
              </chunk>
            </out>
            <choose><when>
              <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
              <out><b pos="1"/></out>
            </when></choose>
          </when>
          <otherwise>
            <call-macro n="stash_case_in_ntags"><with-param pos="2"/></call-macro>
            <out>
              <chunk name="det" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu><clip pos="1" side="tl" part="whole"/></lu>
              </chunk>
              <b pos="1"/>
              <chunk name="n" case="caseSecondWord">
                <tags><tag><var n="ntags"/></tag></tags>
                <lu><clip pos="2" side="tl" part="whole"/></lu>
              </chunk>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="et stort/høyt antall → mange
                   idiomatic rule">
      <pattern>
        <pattern-item n="et"/>
        <pattern-item n="stort_høyt"/>
        <pattern-item n="antall"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose>
          <when c="mange">
            <test><or>
              <equal><clip pos="3" side="tl" part="lemh"/><lit v="fleire"/></equal>
              <!-- TODO -->
              <equal><clip pos="3" side="tl" part="lemh"/><clip pos="3" side="tl" part="lemh"/></equal>
            </or></test>
            <out>
              <chunk name="adj" case="caseFirstWord">
                <tags><tag><lit-tag v="adj.sint.pst.un.pl.ind"/></tag></tags>
                <lu>
                  <lit v="mange"/>
                  <lit-tag v="adj.sint.pst.un.pl.ind"/>
                </lu>
              </chunk>
            </out>
            <choose><when><test><not><equal><b pos="1"/><lit v=" "/></equal></not></test><out><b pos="1"/></out></when></choose>
            <choose><when><test><not><equal><b pos="2"/><lit v=" "/></equal></not></test><out><b pos="2"/></out></when></choose>
          </when>
          <otherwise>
            <call-macro n="stash_case_in_ntags"><with-param pos="3"/></call-macro>
            <out>
              <chunk name="det" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu><clip pos="1" side="tl" part="whole"/></lu>
              </chunk>
              <b pos="1"/>
              <chunk name="adj" case="caseSecondWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu><clip pos="2" side="tl" part="whole"/></lu>
              </chunk>
              <b pos="2"/>
              <chunk name="n" case="caseSecondWord">
                <tags><tag><var n="ntags"/></tag></tags>
                <lu><clip pos="3" side="tl" part="whole"/></lu>
              </chunk>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="et antall på [2 millioner] → ∅ [2 millionar]
                   idiomatic rule">
      <pattern>
        <pattern-item n="et"/>
        <pattern-item n="antall"/>
        <pattern-item n="på"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose><when>
          <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
          <out><b pos="1"/></out>
        </when></choose>
        <choose><when>
          <test><not><equal><b pos="2"/><lit v=" "/></equal></not></test>
          <out><b pos="2"/></out>
        </when></choose>
      </action>
    </rule>

    <rule comment="totalt antall → det samla talet
                   idiomatic rule">
      <pattern>
        <pattern-item n="totalt"/>
        <pattern-item n="antall"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu>
              <lit v="den"/>
              <lit-tag v="det.dem.nt.sg"/>
            </lu>
            <b/>
            <lu>
              <lit v="samle"/>
              <lit-tag v="vblex.pp"/>
            </lu>
            <b pos="1"/>
            <lu>
              <lit v="tal"/>
              <lit-tag v="n.nt.sg.def"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="påfølgende uke → den følgjande veka">
      <pattern>
        <pattern-item n="påfølgende"/>
        <pattern-item n="nind_time"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose>
          <when>
            <test><equal><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></equal></test>
            <let><var n="det_gen"/><lit-tag v="un"/></let>
          </when>
          <when>
            <test><or>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="m"/></equal>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="f"/></equal>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="nt"/></equal>
            </or></test>
            <let><var n="det_gen"/><clip pos="2" side="tl" part="gen"/></let>
          </when>
          <otherwise>
            <let><var n="det_gen"/><lit-tag v="m"/></let>
          </otherwise>
        </choose>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu>
              <lit v="den"/>
              <lit-tag v="det.dem"/>
              <var n="det_gen"/>
              <clip pos="2" side="tl" part="nbr"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_adj"/>
              <lit-tag v="pst.un.sp.def"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="gen"/>
              <clip pos="2" side="tl" part="nbr"/>
              <lit-tag v="def"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="klokken 14 → klokka 14">
      <pattern>
        <pattern-item n="klokken"/>
        <pattern-item n="detqntun"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="klokken 14. → klokka 14.">
      <pattern>
        <pattern-item n="klokken"/>
        <pattern-item n="detqntun"/>
        <pattern-item n="remspc"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <let><clip pos="3" side="tl" part="a_remspc"/><lit v=""/></let>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <b pos="2"/>
          <chunk name="default" case="caseFirstWord">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- idiomatic NGEN NIND → (DET) ADJ NDEF -->
    <rule comment="britenes statsminister → (den) britiske statsministeren">
      <pattern>
        <pattern-item n="britenes"/>
        <pattern-item n="nind_indgen"/>
      </pattern>
      <action>
        <let><var n="brlemma"/><clip pos="1" side="sl" part="lemh"/></let>
        <modify-case><var n="brlemma"/><lit v="aa"/></modify-case>
        <choose>
          <when>
            <test><equal><var n="brlemma"/><lit v="brite"/></equal></test>
            <let>        <var n="brlemma"/><lit v="britisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="russer"/></equal></test>
            <let>        <var n="brlemma"/><lit v="russisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="sveitser"/></equal></test>
            <let>        <var n="brlemma"/><lit v="sveitsisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="same"/></equal></test>
            <let>        <var n="brlemma"/><lit v="samisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="india"/></equal></test>
            <let>        <var n="brlemma"/><lit v="indisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="kina"/></equal></test>
            <let>        <var n="brlemma"/><lit v="kinesisk"/></let>
          </when>
          <otherwise>
            <reject-current-rule shifting="no"/>
          </otherwise>
        </choose>
        <choose>
          <when>
            <test><or>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="side"/></equal>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="utmelding"/></equal>
            </or></test>
            <let><var n="c_name"/><lit v="adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.mf.sg.ind"/></let>
            <let><var n="defnes"/><lit-tag v="ind"/></let>
          </when>
          <otherwise>
            <let><var n="c_name"/><lit v="needdet_adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.un.sp.def"/></let>
            <let><var n="defnes"/><lit-tag v="def"/></let>
          </otherwise>
        </choose>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <let><clip pos="2" side="tl" part="art"/><var n="defnes"/></let>
        <call-macro n="stash_case_in_ntags"><with-param pos="2"/></call-macro>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <var n="grau_aux"/>
            <lu>
              <var n="brlemma"/>
              <var n="adjtags"/>
            </lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- Verbs -->
    <rule comment="VBLEX.PASV.FV: leses => vert lest
                   beholdes [ikke] => vert [ikkje] haldt på
                   synes => synest

                   TODO: verb probably should lose possible uppercasing,
                   although it's unlikely to find the passive verb in
                   the beginning of a sentence

                   If we translate into a pstv verb, output like the regular VBLEX rule.

                   If we don't see an infinitive marker in t2x, the aux gets
                   deleted again.">
      <pattern>
        <pattern-item n="vblexpassfv"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="1"/></call-macro>

        <choose>
          <when>
            <test><or>
              <equal><clip pos="1" side="tl" part="a_verb"/><lit-tag v="vblex.pstv"/></equal>
              <equal><clip pos="1" side="sl" part="a_verb"/><lit-tag v="vblex.pstv"/></equal>
            </or></test>
            <out>
              <chunk name="vblex" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <var n="c_lemq"/>
                </lu>
              </chunk>
            </out>
          </when>
          <otherwise>
            <call-macro n="set_aux"><with-param pos="1"/></call-macro>
            <!-- todo: why do I get a bus error when calling set_aux2 here? -->
            <choose c="Only movable past an NP if no lemq or not in np-immovable-verb list">
              <when>
                <test><or>
                  <in><clip pos="1" side="tl" part="lemh"/><list n="np-immovable-verb"/></in>
                  <not><equal><clip pos="1" side="tl" part="lemq"/><lit v=""/></equal></not>
                </or></test>
                <let><var n="movable_tag"/><lit v=""/></let>
              </when>
              <otherwise><let><var n="movable_tag"/><lit-tag v="np-movable"/></let></otherwise>
            </choose>
            <out>
              <chunk name="aux" case="caseFirstWord">
                <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
                <var n="aux"/>
              </chunk>
              <b/>
              <chunk name="adj2vblex">
                <tags>
                  <tag><lit-tag v="adj.pp.nt.sg.ind"/></tag>
                  <tag><var n="movable_tag"/></tag>
                </tags>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <lit-tag v="adj.pp.nt.sg.ind"/>
                  <var n="c_lemq"/>
                </lu>
              </chunk>
            </out>
          </otherwise>
        </choose>
        <out><var n="out_lemq"/></out>
      </action>
    </rule>

    <rule comment="VBLEXPP
                   This rule has to be above the regular vblex rule,
                   override lemq-moving for pp verbs.">
      <pattern>
        <pattern-item n="vblexpp"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let><var n="c_name"/><lit v="vblex"/></let>
        <choose><when>
          <test><or>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="bli"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="vere"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="ha"/></equal>
          </or></test>
          <let><var n="c_name"/><lit v="aux"/></let>
        </when></choose>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_verb"/>
              <clip pos="1" side="tl" part="temps"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="VBLEX
                   If we have nn multiwords, these need the lemq after the
                   inflected part. All lemq's include their own initial space.
                   This rule has to be below the passive rule.">
      <pattern>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="flush-mwe-queue"><with-param pos="1"/></call-macro>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="1"/></call-macro>
        <let><var n="c_name"/><lit v="vblex"/></let>
        <choose><when>
          <test><or>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="bli"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="vere"/></equal>
            <equal><clip pos="1" side="tl" part="lemh"/><lit v="ha"/></equal>
          </or></test>
          <let><var n="c_name"/><lit v="aux"/></let>
        </when></choose>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
