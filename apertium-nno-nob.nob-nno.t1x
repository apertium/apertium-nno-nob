    <rule comment="hvilken som helst by → kva by som helst
                   put lemq after">
      <pattern>
        <pattern-item n="hvilken_som_helst"/>
        <pattern-item n="n_np"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="out_gen-prep"><with-param pos="2"/></call-macro>
        <call-macro n="stash_case_in_ntags1"><with-param pos="2"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="adj" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="lemh"/><clip pos="1" side="tl" part="tags"/><var n="c_lemq"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="nn" case="caseSecondWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <var n="out_lemq"/>
        </out>
      </action>
    </rule>
    <rule comment="en hvilken som helst by → kva by som helst
                   put lemq after
                   and drop DETQNT (should we instead tag the adj as det-drop and do it in t2x?)">
      <pattern>
        <pattern-item n="detqnt"/>
        <pattern-item n="hvilken_som_helst"/>
        <pattern-item n="n_np"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <call-macro n="out_gen-prep"><with-param pos="3"/></call-macro>
        <call-macro n="stash_case_in_ntags1"><with-param pos="3"/></call-macro>
        <call-macro n="set_lemq"><with-param pos="2"/></call-macro>
        <out>
          <chunk name="adj" case="caseFirstWord">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="lemh"/><clip pos="2" side="tl" part="tags"/><var n="c_lemq"/></lu>
          </chunk>
          <b pos="1"/>
          <chunk name="nn" case="caseSecondWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
          <var n="out_lemq"/>
        </out>
        <choose><when><test><not><equal><b pos="2"/><lit v=" "/></equal></not></test><out><b pos="3"/></out></when></choose>
      </action>
    </rule>

    <rule comment="en hvilken som helst annen → kva som helst annan
                   drop DETQNT (should we instead tag the adj as det-drop and do it in t2x?)">
      <pattern>
        <pattern-item n="detqnt"/>
        <pattern-item n="hvilken_som_helst_annen"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose><when>
          <test><equal><clip pos="2" side="tl" part="lemh"/><lit v="alle andre"/></equal></test>
          <let><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></let>
          <let><clip pos="2" side="tl" part="gen"/><lit-tag v="un"/></let>
        </when></choose>
        <out>
          <chunk name="det2adj" case="caseFirstWord">
            <tags><tag><clip pos="2" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <choose><when><test><not><equal><b pos="1"/><lit v=" "/></equal></not></test><out><b pos="1"/></out></when></choose>
      </action>
    </rule>

    <rule comment="anført av X → med X i spissen
                   Sets variable `mwe-queue`, which has to be output before the next finite verb or comma or period.">
      <pattern>
        <pattern-item n="anført"/>
        <pattern-item n="av"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="pr" case="caseFirstWord">
            <tags><tag><lit-tag v="pr.med"/></tag></tags>
            <lu>
              <lit v="med"/>
              <lit-tag v="pr"/>
            </lu>
          </chunk>
        </out>
        <choose><when>
          <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
          <out><b pos="1"/></out>
        </when></choose>
        <let>
          <var n="mwe-queue"/>
          <chunk name="default">
            <tags><tag><lit-tag v="default"/></tag></tags>
            <lu>
              <lit v="i"/>
              <lit-tag v="pr"/>
            </lu>
            <b/>
            <lu>
              <lit v="spiss"/>
              <lit-tag v="n.m.sg.def"/>
            </lu>
          </chunk>
        </let>
      </action>
    </rule>

    <rule comment="har angivelig → skal ha">
      <pattern>
        <pattern-item n="har"/>
        <pattern-item n="angivelig"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="aux" case="caseFirstWord">
            <tags><tag><clip pos="1" side="tl" part="tags"/></tag></tags>
            <lu>
              <lit v="skulle"/>
              <clip pos="1" side="tl" part="tags"/>
            </lu>
          </chunk>
          <b pos="1"/>
          <chunk name="aux" case="caseSecondWord">
            <tags><tag><lit-tag v="vblex.inf"/></tag></tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <lit-tag v="vblex.inf"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="påfølgende uke → den følgjande veka">
      <pattern>
        <pattern-item n="påfølgende"/>
        <pattern-item n="nind_time"/>
      </pattern>
      <action>
        <choose><when><test><not><equal><clip pos="2" side="tl" part="a_cmp"/><lit v=""/></equal></not></test><reject-current-rule shifting="no"/></when></choose>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose>
          <when>
            <test><equal><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></equal></test>
            <let><var n="det_gen"/><lit-tag v="un"/></let>
          </when>
          <when>
            <test><or>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="m"/></equal>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="f"/></equal>
              <equal><clip pos="2" side="tl" part="gen"/><lit-tag v="nt"/></equal>
            </or></test>
            <let><var n="det_gen"/><clip pos="2" side="tl" part="gen"/></let>
          </when>
          <otherwise>
            <let><var n="det_gen"/><lit-tag v="m"/></let>
          </otherwise>
        </choose>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu>
              <lit v="den"/>
              <lit-tag v="det.dem"/>
              <var n="det_gen"/>
              <clip pos="2" side="tl" part="nbr"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_adj"/>
              <lit-tag v="pst.un.sp.def"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="gen"/>
              <clip pos="2" side="tl" part="nbr"/>
              <lit-tag v="def"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="klokken 14 → klokka 14">
      <pattern>
        <pattern-item n="klokken"/>
        <pattern-item n="detqntun"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="klokken 14. → klokka 14.">
      <pattern>
        <pattern-item n="klokken"/>
        <pattern-item n="detqntun"/>
        <pattern-item n="remspc"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <let><clip pos="3" side="tl" part="a_remspc"/><lit v=""/></let>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><lit-tag v="adv"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
          <chunk name="sent">
            <tags><tag><clip pos="3" side="tl" part="tags"/></tag></tags>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- idiomatic NGEN NIND → (DET) ADJ NDEF -->
    <rule comment="britenes statsminister → (den) britiske statsministeren
                   TODO: move to LSX">
      <pattern>
        <pattern-item n="britenes"/>
        <pattern-item n="nind_indgen"/>
      </pattern>
      <action>
        <let><var n="brlemma"/><clip pos="1" side="sl" part="lemh"/></let>
        <modify-case><var n="brlemma"/><lit v="aa"/></modify-case>
        <choose>
          <when>
            <test><equal><var n="brlemma"/><lit v="brite"/></equal></test>
            <let>        <var n="brlemma"/><lit v="britisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="russer"/></equal></test>
            <let>        <var n="brlemma"/><lit v="russisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="sveitser"/></equal></test>
            <let>        <var n="brlemma"/><lit v="sveitsisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="same"/></equal></test>
            <let>        <var n="brlemma"/><lit v="samisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="india"/></equal></test>
            <let>        <var n="brlemma"/><lit v="indisk"/></let>
          </when>
          <when>
            <test><equal><var n="brlemma"/><lit v="kina"/></equal></test>
            <let>        <var n="brlemma"/><lit v="kinesisk"/></let>
          </when>
          <otherwise>
            <reject-current-rule shifting="no"/>
          </otherwise>
        </choose>
        <call-macro n="set_caseFirstWord2">
          <with-param pos="1"/>
          <with-param pos="2"/>
        </call-macro>
        <choose>
          <when>
            <test><or>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="side"/></equal>
              <equal><clip pos="2" side="tl" part="lemh"/><lit v="utmelding"/></equal>
            </or></test>
            <let><var n="c_name"/><lit v="adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.mf.sg.ind"/></let>
            <let><clip pos="2" side="tl" part="art"/><lit-tag v="ind"/></let>
          </when>
          <otherwise>
            <let><var n="c_name"/><lit v="needdet_adj_n"/></let>
            <let><var n="adjtags"/><lit-tag v="adj.pst.un.sp.def"/></let>
            <let><clip pos="2" side="tl" part="art"/><lit-tag v="def"/></let>
          </otherwise>
        </choose>
        <call-macro n="stash_case_in_ntags1"><with-param pos="2"/></call-macro>
        <out>
          <chunk namefrom="c_name" case="caseFirstWord">
            <tags><tag><var n="ntags"/></tag></tags>
            <var n="grau_aux"/>
            <lu>
              <var n="brlemma"/>
              <var n="adjtags"/>
            </lu>
            <b pos="1"/>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- Verbs -->
    <rule comment="VBLEX
                   If we have nn multiwords, these need the lemq after the
                   inflected part. All lemq's include their own initial space.">
      <pattern>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let><var n="c_name"/><lit v=""/></let>
        <let><var n="cmp-lefts"/><lit v=""/></let>
        <call-macro n="out_verb"><with-param pos="1"/></call-macro>
      </action>
    </rule>

    <rule comment="CMP VBLEX">
      <pattern>
        <pattern-item n="cmp"/>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <let><var n="c_name"/><lit v="cmp_"/></let>
        <let>
          <var n="cmp-lefts"/>
          <lu><clip pos="1" side="tl" part="whole"/></lu>
        </let>
        <call-macro n="out_verb"><with-param pos="2"/></call-macro>
      </action>
    </rule>
    <rule comment="CMP CMP VBLEX">
      <pattern>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <let><var n="c_name"/><lit v="cmp_cmp_"/></let>
        <let>
          <var n="cmp-lefts"/>
          <concat>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </concat>
        </let>
        <call-macro n="out_verb"><with-param pos="3"/></call-macro>
      </action>
    </rule>
    <rule comment="CMP CMP CMP VBLEX">
      <pattern>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord4"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/><with-param pos="4"/></call-macro>
        <let><var n="c_name"/><lit v="cmp_cmp_cmp_"/></let>
        <let>
          <var n="cmp-lefts"/>
          <concat>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
          </concat>
        </let>
        <call-macro n="out_verb"><with-param pos="4"/></call-macro>
      </action>
    </rule>
    <rule comment="CMP CMP CMP CMP VBLEX">
      <pattern>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="cmp"/>
        <pattern-item n="vblex"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord5"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/><with-param pos="4"/><with-param pos="5"/></call-macro>
        <let><var n="c_name"/><lit v="cmp_cmp_cmp_cmp_"/></let>
        <let>
          <var n="cmp-lefts"/>
          <concat>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
            <lu><clip pos="3" side="tl" part="whole"/></lu>
            <lu><clip pos="4" side="tl" part="whole"/></lu>
          </concat>
        </let>
        <call-macro n="out_verb"><with-param pos="5"/></call-macro>
      </action>
    </rule>

    <rule comment="INFM">
      <pattern>
        <pattern-item n="infm"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="infm" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="tl" part="tags"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      </action>
    </rule>
    <rule comment="ADV">
      <pattern>
        <pattern-item n="adv"/>
      </pattern>
      <action><choose><when c="do nothing if empty target lemma">
        <test><not><equal><clip pos="1" side="tl" part="lemh"/><lit v=""/></equal></not></test>
        <call-macro n="set_caseFirstWord"><with-param pos="1"/></call-macro>
        <let><var n="advtags"/><clip pos="1" side="tl" part="tags"/></let>
        <choose>
          <when c="Only tag as movable if not in list of bound-adv's">
            <test><in><clip pos="1" side="tl" part="lemh"/><list n="bound-adv"/></in></test>
            <append n="advtags">
              <lit-tag v="bound-adv"/>
            </append>
          </when>
          <when>
            <test><in><clip pos="1" side="tl" part="lemh"/><list n="free-adv"/></in></test>
            <append n="advtags">
              <lit-tag v="free-adv"/>
            </append>
          </when>
          <otherwise>
            <append n="advtags">
              <lit-tag v="sent-adv"/>
            </append>
          </otherwise>
        </choose>
        <choose><when c="Hack used for PoS change «ubevoktet →  utan tilsyn» (should probably happen in lsx)">
          <test><in><clip pos="1" side="tl" part="lemh"/><list n="n-movable-adv"/></in></test>
          <append n="advtags">
            <lit-tag v="n-movable"/>
          </append>
        </when></choose>
        <out>
          <chunk name="adv" case="caseFirstWord">
            <tags><tag><var n="advtags"/></tag></tags>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
          </chunk>
        </out>
        <let><var n="prevlem"/><clip pos="1" side="tl" part="lem"/></let>
      </when></choose></action>
    </rule>
    <rule comment="så-ADV lenge-ADV/ADJ: så lenge → så lenge">
      <pattern>
        <pattern-item n="så-adv"/>
        <pattern-item n="lenge"/>
      </pattern>
      <action>
        <call-macro n="set_caseFirstWord2"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <let><var n="advtags"/><clip pos="1" side="tl" part="tags"/></let>
        <!-- not movable -->
        <out>
          <chunk name="adv" case="caseFirstWord">
