<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <!-- nb=>nn -->
  <mode name="nb-nn_e_nocp" install="no"> <!-- infinitive verbs ending in -e, no compounding (do make nocp first) -->
    <pipeline>
      <program name="lt-proc -w">
        <file name="nb-nn.automorf-no-cp.bin"/>
      </program>
      <program name="cg-proc -w">
	<file name="nb-nn.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="nb-nn.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-nn-nb.nb-nn.t1x"/>
        <file name="nb-nn.t1x.bin"/>
        <file name="nb-nn.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-nn-nb.nb-nn.t2x"/>
        <file name="nb-nn.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-nn-nb.nb-nn.t3x"/>
        <file name="nb-nn.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="nb-nn_e.autogen-no-cp.bin"/>
      </program>
    </pipeline>
  </mode>

